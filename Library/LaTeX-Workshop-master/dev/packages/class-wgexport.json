{
  "includes": {
    "class-standalone": [],
    "wargame": [],
    "class-beamer": [
      "beamer",
      "beamer=true"
    ],
    "multido": [
      "multido",
      "multido=true"
    ],
    "preview": [
      "preview",
      "preview=true"
    ],
    "pstricks": [
      "pstricks",
      "pstricks=true"
    ],
    "tikz": [
      "tikz",
      "tikz=true"
    ],
    "varwidth": [
      "varwidth",
      "varwidth=true"
    ]
  },
  "macros": {
    "info{}{}{}": {
      "snippet": "info{${1:image-name}}{${2:category}}{${3:subcategory}}"
    },
    "info*{}{}{}": {
      "snippet": "info*{${1:image-name}}{${2:category}}{${3:subcategory}}"
    },
    "chitimages{}": {
      "snippet": "chitimages{${1:counter}}"
    },
    "chitimages[]{}": {
      "snippet": "chitimages[${2:faction}]{${1:counter}}"
    },
    "chitimages[][]{}": {
      "snippet": "chitimages[${2:faction}][${3:TikZ keys}]{${1:counter}}"
    },
    "doublechitimages{}": {
      "snippet": "doublechitimages{${1:counter}}"
    },
    "doublechitimages[]{}": {
      "snippet": "doublechitimages[${2:faction}]{${1:counter}}"
    },
    "doublechitimages[][]{}": {
      "snippet": "doublechitimages[${2:faction}][${3:TikZ keys}]{${1:counter}}"
    },
    "dice{}{}{}": {
      "snippet": "dice{${1:name}}{${2:name}}{${3:list}}"
    },
    "dice[]{}{}{}": {
      "snippet": "dice[${4:tikz-options}]{${1:name}}{${2:name}}{${3:list}}"
    },
    "dice[][]{}{}{}": {
      "snippet": "dice[${4:tikz-options}][${5:node-options}]{${1:name}}{${2:name}}{${3:list}}"
    },
    "battlemarkers{}": {
      "snippet": "battlemarkers{${1:arg}}"
    },
    "battlemarkers[]{}": {
      "snippet": "battlemarkers[${2:opt}]{${1:arg}}"
    },
    "battlemarkers[][]{}": {
      "snippet": "battlemarkers[${2:opt1}][${3:opt2}]{${1:arg}}"
    },
    "oddsmarkers{}": {
      "snippet": "oddsmarkers{${1:arg}}"
    },
    "oddsmarkers[]{}": {
      "snippet": "oddsmarkers[${2:opt}]{${1:arg}}"
    },
    "oddsmarkers[][]{}": {
      "snippet": "oddsmarkers[${2:opt1}][${3:opt2}]{${1:arg}}"
    },
    "resultmarkers{}": {
      "snippet": "resultmarkers{${1:arg}}"
    },
    "resultmarkers[]{}": {
      "snippet": "resultmarkers[${2:opt}]{${1:arg}}"
    },
    "resultmarkers[][]{}": {
      "snippet": "resultmarkers[${2:opt1}][${3:opt2}]{${1:arg}}"
    },
    "commonicons{}{}": {
      "snippet": "commonicons{${1:arg1}}{${2:arg2}}"
    },
    "multimathsep": {
      "option": "math=true"
    },
    "multidisplaymathsep": {
      "option": "math=true"
    }
  },
  "envs": {
    "imagelist": {},
    "imagelist[]": {
      "name": "imagelist",
      "snippet": "[${1:jobname}]"
    },
    "boardimage{}{}": {
      "name": "boardimage",
      "snippet": "{${1:image-name}}{${2:subcategory}}"
    },
    "boardimage[]{}{}": {
      "name": "boardimage",
      "snippet": "[${3:classification}]{${1:image-name}}{${2:subcategory}}"
    },
    "standaloneframe": {
      "option": "beamer=true"
    },
    "standaloneframe<>": {
      "name": "standaloneframe",
      "snippet": "<${1:overlay spec}>",
      "option": "beamer=true"
    },
    "standaloneframe<>[]": {
      "name": "standaloneframe",
      "snippet": "<${2:overlay spec}>[${1:<default overlay spec>}]",
      "option": "beamer=true"
    },
    "standaloneframe<>[][]": {
      "name": "standaloneframe",
      "snippet": "<${3:overlay spec}>[${1:<default overlay spec>}][${2:options}]",
      "option": "beamer=true"
    },
    "standaloneframe<>[][]{}": {
      "name": "standaloneframe",
      "snippet": "<${4:overlay spec}>[${2:<default overlay spec>}][${3:options}]{${1:title}}",
      "option": "beamer=true"
    },
    "standaloneframe<>[][]{}{}": {
      "name": "standaloneframe",
      "snippet": "<${5:overlay spec}>[${3:<default overlay spec>}][${4:options}]{${1:title}}{${2:subtitle}}",
      "option": "beamer=true"
    },
    "standaloneframe[]": {
      "name": "standaloneframe",
      "snippet": "[${1:options}]",
      "option": "beamer=true"
    },
    "standaloneframe[][]": {
      "name": "standaloneframe",
      "snippet": "[${1:<default overlay spec>}][${2:options}]",
      "option": "beamer=true"
    },
    "standaloneframe[][]{}": {
      "name": "standaloneframe",
      "snippet": "[${2:<default overlay spec>}][${3:options}]{${1:title}}",
      "option": "beamer=true"
    },
    "standaloneframe[][]{}{}": {
      "name": "standaloneframe",
      "snippet": "[${3:<default overlay spec>}][${4:options}]{${1:title}}{${2:subtitle}}",
      "option": "beamer=true"
    },
    "standaloneframe[]{}": {
      "name": "standaloneframe",
      "snippet": "[${2:options}]{${1:title}}",
      "option": "beamer=true"
    },
    "standaloneframe[]{}{}": {
      "name": "standaloneframe",
      "snippet": "[${3:options}]{${1:title}}{${2:subtitle}}",
      "option": "beamer=true"
    },
    "standaloneframe{}": {
      "name": "standaloneframe",
      "snippet": "{${1:title}}",
      "option": "beamer=true"
    },
    "multimath": {
      "option": "math=true"
    },
    "multidisplaymath": {
      "option": "math=true"
    }
  },
  "options": [
    "noterrainpic",
    "terrainpic",
    "beamer",
    "border=",
    "border={${1:<l/r> <t/b>}}",
    "border={${1:<l> <b> <r> <t>}}",
    "class=${1:class name}",
    "convert",
    "convert={${1:conversion options}}",
    "crop",
    "float",
    "float",
    "gif",
    "gif={${1:conversion options}}",
    "ignoreempty",
    "ignorerest",
    "jpg",
    "jpg={${1:conversion options}}",
    "margin=",
    "math",
    "multi",
    "multi={${1:envname1,envname2,...}}",
    "multido",
    "png",
    "png={${1:conversion options}}",
    "preview",
    "pstricks",
    "tikz",
    "varwidth",
    "varwidth="
  ],
  "keyvals": []
}