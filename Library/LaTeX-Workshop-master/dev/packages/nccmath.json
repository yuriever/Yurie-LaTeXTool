{
  "includes": {
    "amsmath": []
  },
  "macros": {
    "intertext[]{}": {
      "snippet": "intertext[${2:distance}]{${1:word or phrase}}"
    },
    "dmulticolumn{}{}{}": {
      "snippet": "dmulticolumn{${1:count}}{${2:preamble}}{${3:formula}}"
    },
    "useshortskip": {},
    "nr": {},
    "nr[]": {
      "snippet": "nr[${1:dist}]"
    },
    "nr*": {},
    "nr*[]": {
      "snippet": "nr*[${1:dist}]"
    },
    "mrel{}": {
      "snippet": "mrel{${1:formula}}"
    },
    "underrel{}{}": {
      "snippet": "underrel{${1:base}}{${2:bottom}}"
    },
    "medmath{}": {
      "snippet": "medmath{${1:formula}}"
    },
    "medop{}": {
      "snippet": "medop{${1:operator}}"
    },
    "medint{}": {
      "snippet": "medint{${1:operator}}"
    },
    "medintcorr{}": {
      "snippet": "medintcorr{${1:length}}"
    },
    "mfrac{}{}": {
      "snippet": "mfrac{${1:num}}{${2:den}}"
    },
    "mbinom{}{}": {
      "snippet": "mbinom{${1:above}}{${2:below}}"
    },
    "eq{}": {
      "snippet": "eq{${1:formula}}"
    },
    "eq*{}": {
      "snippet": "eq*{${1:formula}}"
    },
    "eqs{}": {
      "snippet": "eqs{${1:formulas}}"
    },
    "eqs[]{}": {
      "snippet": "eqs[${2:dist}]{${1:formulas}}"
    },
    "eqs*{}": {
      "snippet": "eqs*{${1:formulas}}"
    },
    "eqs*[]{}": {
      "snippet": "eqs*[${2:dist}]{${1:formulas}}"
    },
    "eqalign{}": {
      "snippet": "eqalign{${1:formulas}}"
    },
    "eqalign*{}": {
      "snippet": "eqalign*{${1:formulas}}"
    }
  },
  "envs": {
    "fleqn": {},
    "fleqn[]": {
      "name": "fleqn",
      "snippet": "[${1:margin}]"
    },
    "ceqn": {},
    "darray{}": {
      "name": "darray",
      "snippet": "{${1:cols}}"
    },
    "darray[]{}": {
      "name": "darray",
      "snippet": "[${2:pos}]{${1:cols}}"
    },
    "medsize": {},
    "mmatrix": {}
  },
  "options": [
    "mediummath",
    "intlimits",
    "nointlimits",
    "sumlimits",
    "nosumlimits",
    "namelimits",
    "nonamelimits",
    "leqno",
    "reqno",
    "centertags",
    "tbtags",
    "cmex10",
    "fleqn",
    "alignedleftspaceyes",
    "alignedleftspaceno",
    "alignedleftspaceyesifneg"
  ],
  "keyvals": []
}