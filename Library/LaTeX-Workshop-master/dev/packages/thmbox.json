{
  "includes": {
    "keyval": []
  },
  "macros": {
    "thmboxoptions{}": {
      "snippet": "thmboxoptions{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "newtheorem[]{}{}": {
      "snippet": "newtheorem[${3:options}]{${1:envname}}{${2:caption}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "newtheorem[]{}{}[]": {
      "snippet": "newtheorem[${3:options}]{${1:envname}}{${2:caption}}[${4:within}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "examplename": {},
    "proofname": {},
    "newboxtheorem{}{}": {
      "snippet": "newboxtheorem{${1:envname}}{${2:caption}}"
    },
    "newboxtheorem[]{}{}": {
      "snippet": "newboxtheorem[${3:options}]{${1:envname}}{${2:caption}}"
    }
  },
  "envs": {
    "thmbox{}": {
      "name": "thmbox",
      "snippet": "{${1:title}}"
    },
    "thmbox[]{}": {
      "name": "thmbox",
      "snippet": "[${2:options}]{${1:title}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "leftbar": {},
    "leftbar[]": {
      "name": "leftbar",
      "snippet": "[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "proof": {},
    "proof[]": {
      "name": "proof",
      "snippet": "[${1:postfix}]"
    },
    "example": {},
    "example[]": {
      "name": "example",
      "snippet": "[${1:label text}]"
    }
  },
  "options": [
    "nothm",
    "nocut",
    "nounderline"
  ],
  "keyvals": [
    [
      "style=",
      "S",
      "M",
      "L",
      "cut",
      "underline",
      "headstyle=${1:code}",
      "titlestyle=${1:code}",
      "bodystyle=${1:code}",
      "leftmargin=",
      "rightmargin=",
      "hskip=",
      "vskip=",
      "thickness="
    ]
  ]
}