{
  "includes": {
    "zref-base": [],
    "keyval": [],
    "kvoptions": []
  },
  "macros": {
    "zexternaldocument{}": {
      "snippet": "zexternaldocument{${1:file}}"
    },
    "zexternaldocument[]{}[]": {
      "snippet": "zexternaldocument[${2:prefix}]{${1:file}}[${3:url}]"
    },
    "zexternaldocument*{}": {
      "snippet": "zexternaldocument*{${1:file}}"
    },
    "zexternaldocument*[]{}[]": {
      "snippet": "zexternaldocument*[${2:prefix}]{${1:file}}[${3:url}]"
    },
    "zxrsetup{}": {
      "snippet": "zxrsetup{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "ext=${1:extension}",
      "tozreflabel",
      "urluse",
      "verbose"
    ]
  ]
}