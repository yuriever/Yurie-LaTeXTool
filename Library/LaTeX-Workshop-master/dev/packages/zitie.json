{
  "includes": {
    "l3draw": [],
    "l3keys2e": [],
    "zhlipsum": [
      "enable-zhlipsum",
      "enable-zhlipsum=true"
    ]
  },
  "macros": {
    "zitienewfont{{}{}": {
      "snippet": "zitienewfont{{${1:font family}}{${2:font name},...}}"
    },
    "zitienewfont{{}{}{}": {
      "snippet": "zitienewfont{{${1:font family}}{${2:font name}}{${3:font features},...}}"
    },
    "zitienewfont*{}{}{}": {
      "snippet": "zitienewfont*{${1:font family}}{${2:font name}}{${3:font features}}"
    },
    "zitieCJKfamily{}": {
      "snippet": "zitieCJKfamily{${1:font family}}"
    },
    "framesingle{}": {
      "snippet": "framesingle{${1:character}}"
    },
    "framesingle[]{}": {
      "snippet": "framesingle[${2:options}]{${1:character}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "framezi{}": {
      "snippet": "framezi{${1:characters}}"
    },
    "framezi[]{}": {
      "snippet": "framezi[${2:options}]{${1:characters}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "framezi*{}": {
      "snippet": "framezi*{${1:characters}}"
    },
    "framezi*[]{}": {
      "snippet": "framezi*[${2:options}]{${1:characters}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "framerange{}": {
      "snippet": "framerange{${1:hex range}}"
    },
    "framerange[]{}": {
      "snippet": "framerange[${2:options}]{${1:hex range}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "framezifile{}": {
      "snippet": "framezifile{${1:file}}"
    },
    "framezifile[]{}": {
      "snippet": "framezifile[${2:options}]{${1:file}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "framezifile*{}": {
      "snippet": "framezifile*{${1:file}}"
    },
    "framezifile*[]{}": {
      "snippet": "framezifile*[${2:options}]{${1:file}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "frametallrange{}": {
      "snippet": "frametallrange{${1:hex range}}"
    },
    "frametallrange[]{}": {
      "snippet": "frametallrange[${2:options}]{${1:hex range}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "framezitallfile{}": {
      "snippet": "framezitallfile{${1:file}}"
    },
    "framezitallfile[]{}": {
      "snippet": "framezitallfile[${2:options}]{${1:file}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "framezitallfile*{}": {
      "snippet": "framezitallfile*{${1:file}}"
    },
    "framezitallfile*[]{}": {
      "snippet": "framezitallfile*[${2:options}]{${1:file}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "zitiesetup{}": {
      "snippet": "zitiesetup{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "zitiestrokechars{}{}": {
      "snippet": "zitiestrokechars{${1:pdf literal}}{${2:typeset material}}"
    },
    "zitiecolorlet{}{}": {
      "snippet": "zitiecolorlet{${1:name}}{${2:value}}"
    },
    "zitiecolorlet{}[]{}": {
      "snippet": "zitiecolorlet{${1:name}}[${2:model}]{${3:value}}"
    },
    "zitienewprocessorrule{}{}{}": {
      "snippet": "zitienewprocessorrule{${1:processor}}{${2:rule}}{${3:replace}}"
    },
    "zitienewprocessorrule[]{}{}{}": {
      "snippet": "zitienewprocessorrule[${4:arg nums}]{${1:processor}}{${2:rule}}{${3:replace}}"
    },
    "zitienewrule{}{}{}": {
      "snippet": "zitienewrule{${1:processor}}{${2:rule}}{${3:replace}}"
    },
    "zitienewrule[]{}{}{}": {
      "snippet": "zitienewrule[${4:arg nums}]{${1:processor}}{${2:rule}}{${3:replace}}"
    },
    "zitieuseprocessorrule{}{}": {
      "snippet": "zitieuseprocessorrule{${1:processor}}{${2:rule}}"
    },
    "zitieuseprocessorrule[]{}{}": {
      "snippet": "zitieuseprocessorrule[${3:arg nums}]{${1:processor}}{${2:rule}}"
    },
    "zitiebasechar": {},
    "zitiebasecharwidth": {},
    "zitiebasecharheight": {},
    "zitiewidth": {},
    "zitieheight": {},
    "zitieboxwd": {},
    "zitieboxht": {},
    "zitieboxdp": {},
    "zitiefontname": {},
    "zitiexscaleratio": {},
    "zitieyscaleratio": {},
    "zitiebackground": {},
    "zitiebackground[]": {
      "snippet": "zitiebackground[${1:options}]",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "framezhlipsum{}": {
      "snippet": "framezhlipsum{${1:paragraph list}}"
    },
    "framezhlipsum{}[]": {
      "snippet": "framezhlipsum{${1:paragraph list}}[${2:name}]"
    },
    "framezhlipsum[]{}": {
      "snippet": "framezhlipsum[${2:options}]{${1:paragraph list}}"
    },
    "framezhlipsum[]{}[]": {
      "snippet": "framezhlipsum[${2:options}]{${1:paragraph list}}[${3:name}]"
    },
    "zitierawCJKfamily{}": {
      "snippet": "zitierawCJKfamily{${1:font family}}"
    }
  },
  "envs": {
    "zitieframe": {},
    "zitieframe[]": {
      "name": "zitieframe",
      "snippet": "[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
  "options": [
    "enable-zhlipsum",
    "enable-background"
  ],
  "keyvals": [
    [
      "basechar=${1:character}",
      "fontsize=${1:fontsize command}",
      "zihao=${1:字号}",
      "position=",
      "position*={${1:replace}}",
      "anchor=",
      "punctuation=",
      "punctuation*={${1:replace}}",
      "frametype=",
      "resize=",
      "xscale=${1:scale ratio}",
      "yscale=${1:scale ratio}",
      "scale=${1:scale ratio}",
      "width=",
      "height=",
      "holdbasecharheight",
      "holdbasecharwidth",
      "holdbasechar",
      "linewidth=",
      "dashpattern={${1:dim1,dim2,...}}",
      "framearc=",
      "framearc*={${1:dim1}}{${2:dim2}}",
      "framecolor=",
      "framecolor*={${1:models}}{${2:values}}",
      "charcolor=",
      "charcolor*={${1:models}}{${2:values}}",
      "fillcolor=",
      "fillcolor*={${1:models}}{${2:values}}",
      "charstroke=",
      "charstrokespecial=${1:pdf literal}",
      "font=${1:font family}",
      "savefontname",
      "fallback",
      "fallbackfont={${1:fallback font list}}",
      "fallbackfont+={${1:fallback font list}}",
      "filepath={${1:filepath1,filepath2,...}}",
      "filepath+={${1:filepath1,filepath2,...}}",
      "repeat=${1:integer}",
      "break=",
      "tolerance=",
      "tallheight=${1:integer}",
      "validateglyph"
    ],
    [
      "true",
      "on",
      "false",
      "off",
      "next",
      "colboxes=${1:integer}",
      "rowboxes=${1:integer}",
      "framewidth=",
      "frameheight=",
      "boxwidth=",
      "boxheight=",
      "onpaper",
      "ontext",
      "frametype=",
      "linewidth=",
      "framecolor=",
      "framecolor*={${1:models}}{${2:values}}",
      "fillcolor=",
      "fillcolor*={${1:models}}{${2:values}}",
      "dashpattern={${1:dim1,dim2,...}}",
      "xrange={${1:left,right}}",
      "yrange={${1:top,bottom}}"
    ]
  ]
}