{
  "includes": {
    "booktabs": [],
    "multirow": [],
    "longtable": [],
    "makecell": [],
    "array": [],
    "xcolor": [],
    "colortbl": [],
    "luacode": [],
    "penlight": []
  },
  "macros": {
    "settabular{}": {
      "snippet": "settabular{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "lttdebugON": {},
    "lttdebugOFF": {},
    "lttdebugprt": {},
    "MC{}": {
      "snippet": "MC{${1:contents}}"
    },
    "MC[]{}": {
      "snippet": "MC[${2:cell spec}]{${1:contents}}"
    },
    "MC[][]<>{}": {
      "snippet": "MC[${2:cell spec}][${3:override multicol}]<${4:cell format}>{${1:contents}}"
    },
    "MC*{}": {
      "snippet": "MC*{${1:contents}}"
    },
    "MC*[]{}": {
      "snippet": "MC*[${2:cell spec}]{${1:contents}}"
    },
    "MC*[][]<>{}": {
      "snippet": "MC*[${2:cell spec}][${3:override multicol}]<${4:cell format}>{${1:contents}}"
    },
    "setMCrepl{}{}": {
      "snippet": "setMCrepl{${1:column}}{${2:spec}}"
    },
    "setMChordef{}{}": {
      "snippet": "setMChordef{${1:column}}{${2:alignments}}"
    },
    "setMCverdef{}{}": {
      "snippet": "setMCverdef{${1:column}}{${2:alignments}}"
    },
    "addMCsicol{}": {
      "snippet": "addMCsicol{${1:column}}"
    },
    "midrulesat{}": {
      "snippet": "midrulesat{${1:row1,row2,...}}"
    },
    "gmidrule{}": {
      "snippet": "gmidrule{${1:colspecs}}"
    },
    "gcmidrule{}": {
      "snippet": "gcmidrule{${1:colspecs}}"
    },
    "gcmidrules{}": {
      "snippet": "gcmidrules{${1:colspecs}}"
    },
    "cmidrules{}": {
      "snippet": "cmidrules{${1:colspecs}}"
    },
    "midruleX{}": {
      "snippet": "midruleX{${1:keyvals}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "midruleX[]{}": {
      "snippet": "midruleX[${2:o|n|f}]{${1:keyvals}}",
      "keyvalindex": 1,
      "keyvalpos": 1
    },
    "midruleX*{}": {
      "snippet": "midruleX*{${1:keyvals}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "midruleX*[]{}": {
      "snippet": "midruleX*[${2:o|n|f}]{${1:keyvals}}",
      "keyvalindex": 1,
      "keyvalpos": 1
    },
    "resetmidruleX": {},
    "resetmidruleX[]": {
      "snippet": "resetmidruleX[${1:counter}]"
    },
    "forcecolspec{}": {
      "snippet": "forcecolspec{${1:arg}}"
    },
    "oldgcmidrule": {},
    "oldcmidrule": {}
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "nopad",
      "tbrule",
      "rowsep=${1:integer}",
      "colsep=${1:integer}"
    ],
    [
      "step=${1:integer}",
      "rule=",
      "reset",
      "resetnum=${1:integer}",
      "cntr=${1:integer}"
    ]
  ]
}