{
  "includes": {},
  "macros": {
    "ch{}": {
      "snippet": "ch{${1:chapter number}}"
    },
    "ch*{}": {
      "snippet": "ch*{${1:chapter number}}"
    },
    "extraskip": {},
    "added{}": {
      "snippet": "added{${1:words}}"
    },
    "name{}": {
      "snippet": "name{${1:divine name}}"
    },
    "LORD": {},
    "GOD": {},
