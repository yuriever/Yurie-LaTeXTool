    },
    "textscripture{}": {
      "snippet": "textscripture{${1:quotation}}"
    },
    "textscripture[]{}": {
      "snippet": "textscripture[${2:reference}]{${1:quotation}}"
    },
    "textscripture[][]{}": {
      "snippet": "textscripture[${2:reference}][${3:options}]{${1:quotation}}"
    },
    "vs{}": {
      "snippet": "vs{${1:verse number}}"
    }
  },
  "envs": {
    "center[]": {
      "name": "center",
