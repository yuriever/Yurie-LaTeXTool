      "snippet": "polynomialfrac[${3:options}]{${1:numerator coeffs}}{${2:denominator coeffs}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "polynomialstyle{}": {
      "snippet": "polynomialstyle{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
