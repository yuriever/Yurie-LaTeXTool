{
  "includes": {
    "xkeyval": [],
    "luacode": [],
    "tikz": [],
    "luamplib": []
  },
  "macros": {
    "luaplot{}": {
      "snippet": "luaplot{${1:function}}"
    },
    "luaplot[]{}": {
      "snippet": "luaplot[${2:options}]{${1:function}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "luatikzpath{}{}{}{}": {
      "snippet": "luatikzpath{${1:function}}{${2:xmin}}{${3:xmax}}{${4:plot points}}"
    }
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "xmin=${1:number}",
      "xmax=${1:number}",
      "ymin=${1:number}",
      "ymax=${1:number}",
      "plotpts=${1:integer}",
      "hor=",
      "ver=",
      "clr={'${1:color1,color2,...}'}",
      "plotsty={'${1:style1,style2,...}'}",
      "plotoptions={${1:MetaPost code}}"
    ]
  ]
}