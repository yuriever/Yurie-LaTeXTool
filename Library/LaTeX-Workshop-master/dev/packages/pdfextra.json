{
  "includes": {
    "luatex": []
  },
  "macros": {
    "attach[]": {
      "snippet": "attach[${1:name}]"
    },
    "DDDannot{}": {
      "snippet": "DDDannot{${1:name}}"
    },
    "DDDcontext{}": {
      "snippet": "DDDcontext{${1:name}}"
    },
    "DDDview[][]": {
      "snippet": "DDDview[${1:view name}][${2:keyvals}]",
      "keyvalindex": 0,
      "keyvalpos": 1
    },
    "defaultpageactions[]": {
      "snippet": "defaultpageactions[${1:additional action specs}]"
    },
    "defaultpageduration[]": {
      "snippet": "defaultpageduration[${1:duration}]"
    },
    "defaultpagerotate[]": {
      "snippet": "defaultpagerotate[${1:rotation spec}]"
    },
    "dljavascript[]{}": {
      "snippet": "dljavascript[${2:name}]{${1:script}}"
    },
    "duplexdisplay": {},
    "fullscreen": {},
    "hlink[]{}": {
      "snippet": "hlink[${2:action spec}]{${1:text}}"
    },
    "initpageattributes": {},
    "lininglinks": {},
    "nolininglinks": {},
    "openaction[]": {
      "snippet": "openaction[${1:action spec}]"
    },
    "pageactions[]": {
      "snippet": "pageactions[${1:additional action specs}]"
    },
    "pageattributes": {},
    "pageduration[]": {
      "snippet": "pageduration[${1:duration}]"
    },
    "pagerotate[]": {
      "snippet": "pagerotate[${1:rotation spec}]"
    },
    "pdfaction[]": {
      "snippet": "pdfaction[${1:action spec}]"
    },
    "pdfextraloaded": {},
    "render[][]{}": {
      "snippet": "render[${2:name}][${3:options}]{${1:appearance}}",
      "keyvalindex": 1,
      "keyvalpos": 1
    },
    "render[]{}": {
      "snippet": "render[${2:name}]{${1:appearance}}"
    },
    "renditionautoplay": {},
    "RM[][]{}": {
      "snippet": "RM[${2:name}][${3:options}]{${1:appearance}}",
      "keyvalindex": 2,
      "keyvalpos": 1
    },
    "RM[]{}": {
      "snippet": "RM[${2:name}]{${1:appearance}}"
    },
    "sdef{}{}": {
      "snippet": "sdef{${1:string}}{${2:body}}"
    },
    "showattached": {},
    "showoutlines": {},
    "transition[]": {
      "snippet": "transition[${1:transition spec}]"
    },
    "transitions[]": {
      "snippet": "transitions[${1:transition spec}]"
    }
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "projection=",
      "scale=${1:factor}",
      "FOV=${1:number 0-180}",
      "background=${1:OpTeX color}",
      "rendermode=",
      "lighting=",
      "method=",
      "u3dpath=${1:path}",
      "coo=${1:x y z}",
      "roo=${1:number}",
      "c2c=${1:x y z}"
    ],
    [
      "controls",
      "volume=${1:number 0-100}",
      "repeat=${1:integer}",
      "background=${1:OpTeX color}",
      "opacity=${1:factor}",
      "aactions=\\renditionautoplay",
      "name=${1:string}"
    ],
    [
      "activation=",
      "deactivation=",
      "toolbar",
      "views={${1:name1,name2,...}}",
      "scripts={${1:name1,name2,...}}",
      "name=${1:string}",
      "background=${1:OpTeX color}"
    ]
  ]
}