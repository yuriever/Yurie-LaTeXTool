{
  "includes": {
    "xcolor": [],
    "dynkin-diagrams": [],
    "tikzlibrarypositioning": [],
    "tikzlibraryfadings": [],
    "tikzlibraryquotes": []
  },
  "macros": {
    "hasse{}{}": {
      "snippet": "hasse{${1:letter}}{${2:rank}}"
    },
    "hasse[]{}{}": {
      "snippet": "hasse[${3:options}]{${1:letter}}{${2:rank}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "hasseDiagrams{}": {
      "snippet": "hasseDiagrams{${1:type1;type2;...}}"
    },
    "rootSystemHeight{}": {
      "snippet": "rootSystemHeight{${1:count register}}"
    },
    "rootSystemHeight[][]{}": {
      "snippet": "rootSystemHeight[${2:letter}][${3:rank}]{${1:count register}}"
    },
    "rootSystemWidthAtGrade{}{}": {
      "snippet": "rootSystemWidthAtGrade{${1:grade}}{${2:count register}}"
    },
    "rootSystemWidthAtGrade[]{}{}": {
      "snippet": "rootSystemWidthAtGrade[${3:letter}]{${1:grade}}{${2:count register}}"
    },
    "rootSystemWidthAtGrade[][]{}{}": {
      "snippet": "rootSystemWidthAtGrade[${3:letter}][${4:rank}]{${1:grade}}{${2:count register}}"
    },
    "rootSum{}{}{}": {
      "snippet": "rootSum{${1:first-pos}}{${2:second-pos}}{${3:cmd}}"
    },
    "rootSum[]{}{}{}": {
      "snippet": "rootSum[${4:letter}]{${1:first-pos}}{${2:second-pos}}{${3:cmd}}"
    },
    "rootSum[][]{}{}{}": {
      "snippet": "rootSum[${4:letter}][${5:rank}]{${1:first-pos}}{${2:second-pos}}{${3:cmd}}"
    },
    "drawRootAsDynkinSum{}{}": {
      "snippet": "drawRootAsDynkinSum{${1:arg1}}{${2:arg2}}"
    },
    "forAllPositiveRootsInHasseDiagram{}": {
      "snippet": "forAllPositiveRootsInHasseDiagram{${1:command}}"
    },
    "studyHasseDiagramOfRootSystem{}{}": {
      "snippet": "studyHasseDiagramOfRootSystem{${1:letter}}{${2:rank}}"
    },
    "studyHasseDiagramOfRootSystem[]{}{}": {
      "snippet": "studyHasseDiagramOfRootSystem[${3:options}]{${1:letter}}{${2:rank}}"
    },
    "doRootThing{}{}": {
      "snippet": "doRootThing{${1:arg1}}{${2:arg2}}"
    },
    "drawRootDecomp{}{}": {
      "snippet": "drawRootDecomp{${1:first-pos}}{${2:second-pos}}"
    },
    "backwardsGtwo": {},
    "attachDynkin": {},
    "doHasseDiagram{}": {
      "snippet": "doHasseDiagram{${1:arg}}"
    },
    "forscsvlist": {},
    "drpo": {},
    "drmo": {},
    "tdrpt": {},
    "maxy": {},
    "lbl": {},
    "drmt": {},
    "iD": {},
    "xxD": {},
    "maxxD": {}
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "edge length=",
      "edge/.style={${1:TikZ options}}",
      "edge quotes/.style={${1:TikZ options}}",
      "compact edge/.style={${1:TikZ options}}",
      "noncompact edge/.style={${1:TikZ options}}",
      "attach Dynkin diagram",
      "three D",
      "z shift=${1:factor}",
      "top/.style={${1:TikZ options}}",
      "right/.style={${1:TikZ options}}",
      "left/.style={${1:TikZ options}}",
      "show ordering",
      "show name",
      "show widths",
      "show height"
    ]
  ]
}