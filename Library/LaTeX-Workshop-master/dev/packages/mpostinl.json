{
  "includes": {
    "graphicx": [],
    "keyval": [],
    "verbatim": []
  },
  "macros": {
    "mpostsetup{}": {
      "snippet": "mpostsetup{${1:options}}"
    },
    "mpostuse{}": {
      "snippet": "mpostuse{${1:label}}"
    },
    "mpostuse[]{}": {
      "snippet": "mpostuse[${2:options}]{${1:label}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "mpostgetname{}": {
      "snippet": "mpostgetname{${1:label}}"
    },
    "mpostfigurename": {},
    "mpostplaceholder{}": {
      "snippet": "mpostplaceholder{${1:name}}"
    },
    "mpostplaceholder[]{}": {
      "snippet": "mpostplaceholder[${2:type}]{${1:name}}"
    },
    "mpostdisplaylabel{}": {
      "snippet": "mpostdisplaylabel{${1:label}}"
    },
    "mpostfilename": {},
    "mpostfile{}": {
      "snippet": "mpostfile{${1:file}}"
    },
    "mpostfile[]{}": {
      "snippet": "mpostfile[${2:options}]{${1:file}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "mpostdone": {}
  },
  "envs": {
    "mpostfig": {},
    "mpostfig[]": {
      "name": "mpostfig",
      "snippet": "[${1:options}]",
      "keyvalindex": 2,
      "keyvalpos": 0
    },
    "mpostdef": {},
    "mpostdef[]": {
      "name": "mpostdef",
      "snippet": "[${1:options}]",
      "keyvalindex": 3,
      "keyvalpos": 0
    },
    "mposttex": {},
    "mposttex[]": {
      "name": "mposttex",
      "snippet": "[${1:options}]",
      "keyvalindex": 4,
      "keyvalpos": 0
    }
  },
  "options": [
    "draft",
    "final",
    "write",
    "compile",
    "twice",
    "clean",
    "checksum",
    "fonts",
    "prologues=${1:value}",
    "lineno",
    "labelnames",
    "latex",
    "compiler",
    "compiler=${1:compiler}",
    "format",
    "format=${1:tag}",
    "class=${1:class}",
    "classopt={${1:class options}}",
    "mem=${1:mem}",
    "command=${1:command}",
    "now",
    "nowall",
    "nowkeep",
    "globaldef",
    "template=${1:template}",
    "extension=${1:ext}",
    "numberwithin=${1:counter}",
    "beamer",
    "warnunused"
  ],
  "keyvals": [
    [
      "alt={${1:alt text}}",
      "bb=${1:llx lly urx ury}",
      "bbllx=",
      "bblly=",
      "bburx=",
      "bbury=",
      "natwidth=",
      "natheight=",
      "hiresbb",
      "pagebox=",
      "viewport=${1:llx lly urx ury}",
      "trim=${1:llx lly urx ury}",
      "angle=${1:degrees}",
      "origin=",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}",
      "clip",
      "draft",
      "type=${1:file type}",
      "ext=${1:file extension}",
      "read=${1:read-file extension}",
      "command=",
      "quiet",
      "page=${1:page number}",
      "interpolate",
      "decodearray={${1:color array}}"
    ],
    [
      "include"
    ],
    [
      "show",
      "file=${1:file name}",
      "label=${1:label}",
      "opt={${1:includegraphics options}}",
      "now",
      "twice"
    ],
    [
      "global",
      "tex"
    ],
    [
      "global",
      "dual"
    ]
  ]
}