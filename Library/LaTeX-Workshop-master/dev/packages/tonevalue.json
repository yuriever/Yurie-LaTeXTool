{
  "includes": {
    "contour": [],
    "etoolbox": [],
    "listofitems": [],
    "tikz": [],
    "tikzlibrarypositioning": [],
    "tikzlibrarydecorations.markings": [],
    "xcolor": [],
    "xkeyval": [],
    "xstring": []
  },
  "macros": {
    "untpoint{}{}": {
      "snippet": "untpoint{${1:tone value}}{${2:tone name}}"
    },
    "untpoint[]{}{}": {
      "snippet": "untpoint[${3:options}]{${1:tone value}}{${2:tone name}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "linkuntpoints{{}{}{{}{}": {
      "snippet": "linkuntpoints{{${1:toneval1}}{${2:tonename1}}}{{${3:toneval2}}{${4:tonename2}}}"
    },
    "linkuntpoints[]{{}{}{{}{}": {
      "snippet": "linkuntpoints[${5:options}]{{${1:toneval1}}{${2:tonename1}}}{{${3:toneval2}}{${4:tonename2}}}"
    },
    "drawuntpoint{}{}{}{}{}{}{}{}": {
      "snippet": "drawuntpoint{${1:arg1}}{${2:arg2}}{${3:arg3}}{${4:arg4}}{${5:arg5}}{${6:arg6}}{${7:arg7}}{${8:arg8}}"
    },
    "drawuntpoint[]{}{}{}{}{}{}{}{}": {
      "snippet": "drawuntpoint[${9:opt}]{${1:arg1}}{${2:arg2}}{${3:arg3}}{${4:arg4}}{${5:arg5}}{${6:arg6}}{${7:arg7}}{${8:arg8}}"
    },
    "len": {},
    "sendiauToListStr{}": {
      "snippet": "sendiauToListStr{${1:arg}}"
    },
    "sendiaulistStr": {},
    "sendiaulist": {},
    "thesumOfPitchHeights": {},
    "toneVisualisationFontCmd": {},
    "xend": {},
    "xjoinbycomma": {},
    "xstart": {},
    "yend": {},
    "ystart": {}
  },
  "envs": {
    "untVisualisation": {},
    "untVisualisation[]": {
      "name": "untVisualisation",
      "snippet": "[${1:options}]",
      "keyvalindex": 1,
      "keyvalpos": 0
    }
  },
  "options": [
    "fontcmd=${1:font commands}",
    "draft",
    "contourlength=",
    "contournumber=${1:integer}",
    "defaultcolors"
  ],
  "keyvals": [
    [
      "stem",
      "label=",
      "bgcolor=",
      "xshift=",
      "yshift=",
      "scale=${1:factor}",
      "tikzoptions={${1:TikZ options}}"
    ],
    [
      "minmax={${1:min,max}}",
      "scale=${1:factor}",
      "showlabels"
    ]
  ]
}