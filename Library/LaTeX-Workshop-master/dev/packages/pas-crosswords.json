{
  "includes": {
    "fp": [],
    "multido": [],
    "tikz": [],
    "xkeyval": [],
    "xstring": []
  },
  "macros": {
    "blackcase{}{}": {
      "snippet": "blackcase{${1:x}}{${2:y}}"
    },
    "blackcases{}": {
      "snippet": "blackcases{${1:x1/y1,x2/y2,...}}"
    },
    "word{}{}{}": {
      "snippet": "word{${1:x}}{${2:y}}{${3:MOT}}"
    },
    "word[]{}{}{}": {
      "snippet": "word[${4:options}]{${1:x}}{${2:y}}{${3:MOT}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "words{}": {
      "snippet": "words{${1:x1/y1/MOT1,x2/y2.MOT2,...}}"
    },
    "words[]{}": {
      "snippet": "words[${2:options}]{${1:x1/y1/MOT1,x2/y2.MOT2,...}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "gridcross{}": {
      "snippet": "gridcross{${1:MOT1*MOT2,MOT3*MOT4,...}}"
    },
    "printDef{}{}": {
      "snippet": "printDef{${1:h|v}}{${2:la définition}}"
    },
    "printDef[]{}{}": {
      "snippet": "printDef[${3:s}]{${1:h|v}}{${2:la définition}}"
    },
    "symbsep": {},
    "symbnext": {},
    "cRM{}": {
      "snippet": "cRM{${1:text}}"
    },
    "newlist": {},
    "theL": {},
    "thecntdef": {},
    "debutX": {},
    "debutY": {},
    "namecase": {},
    "num": {},
    "pos": {},
    "posX": {},
    "posY": {},
    "pr": {}
  },
  "envs": {
    "crossgrid": {},
    "crossgrid[]": {
      "name": "crossgrid",
      "snippet": "[${1:options}]",
      "keyvalindex": 1,
      "keyvalpos": 0
    }
  },
  "options": [],
  "keyvals": [
    [
      "h",
      "v"
    ],
    [
      "h=${1:nombre de colonnes}",
      "v=${1:nombre de lignes}",
      "pas=${1:scale}"
    ]
  ]
}