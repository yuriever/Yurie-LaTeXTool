{
  "includes": {
    "tikz": [],
    "pgffor": [],
    "xintexpr": [],
    "simplekv": [],
    "listofitems": [],
    "tikzlibrarycalc": [],
    "tikzlibrarydecorations": [],
    "tikzlibrarydecorations.pathmorphing": []
  },
  "macros": {
    "DefinirPoint{}{}": {
      "snippet": "DefinirPoint{${1:noeud}}{${2:coordonnées}}"
    },
    "DefinirPoint{}{}<>": {
      "snippet": "DefinirPoint{${1:noeud}}{${2:coordonnées}}<${3:label}>"
    },
    "DefinirPoint[]{}{}": {
      "snippet": "DefinirPoint[${3:clés}]{${1:noeud}}{${2:coordonnées}}"
    },
    "DefinirPoint[]{}{}<>": {
      "snippet": "DefinirPoint[${3:clés}]{${1:noeud}}{${2:coordonnées}}<${4:label}>"
    },
    "DefinirPoint*{}{}": {
      "snippet": "DefinirPoint*{${1:noeud}}{${2:coordonnées}}"
    },
    "DefinirPoint*{}{}<>": {
      "snippet": "DefinirPoint*{${1:noeud}}{${2:coordonnées}}<${3:label}>"
    },
    "DefinirPoint*[]{}{}": {
      "snippet": "DefinirPoint*[${3:clés}]{${1:noeud}}{${2:coordonnées}}"
    },
    "DefinirPoint*[]{}{}<>": {
      "snippet": "DefinirPoint*[${3:clés}]{${1:noeud}}{${2:coordonnées}}<${4:label}>"
    },
    "DefinirPoints{}": {
      "snippet": "DefinirPoints{${1:liste}}"
    },
    "DefinirPoints[]{}": {
      "snippet": "DefinirPoints[${2:clés}]{${1:liste}}"
    },
    "DefinirPoints*{}": {
      "snippet": "DefinirPoints*{${1:liste}}"
    },
    "DefinirPoints*[]{}": {
      "snippet": "DefinirPoints*[${2:clés}]{${1:liste}}"
    },
    "MarquerPoint{}": {
      "snippet": "MarquerPoint{${1:point}}"
    },
    "MarquerPoint[]{}": {
      "snippet": "MarquerPoint[${2:clés}]{${1:point}}"
    },
    "MarquerPoints{}": {
      "snippet": "MarquerPoints{${1:liste}}"
    },
    "MarquerPoints[]{}": {
      "snippet": "MarquerPoints[${2:clés}]{${1:liste}}"
    },
    "ImagePoint{}": {
      "snippet": "ImagePoint{${1:formule}}"
    },
    "ImagePoint{}<>": {
      "snippet": "ImagePoint{${1:formule}}<${2:label}>"
    },
    "ImagePoint[]{}": {
      "snippet": "ImagePoint[${2:clés}]{${1:formule}}"
    },
    "ImagePoint[]{}<>": {
      "snippet": "ImagePoint[${2:clés}]{${1:formule}}<${3:label}>"
    },
    "ImagePoint*{}": {
      "snippet": "ImagePoint*{${1:formule}}"
    },
    "ImagePoint*{}<>": {
      "snippet": "ImagePoint*{${1:formule}}<${2:label}>"
    },
    "ImagePoint*[]{}": {
      "snippet": "ImagePoint*[${2:clés}]{${1:formule}}"
    },
    "ImagePoint*[]{}<>": {
      "snippet": "ImagePoint*[${2:clés}]{${1:formule}}<${3:label}>"
    },
    "ImagesPoints{}": {
      "snippet": "ImagesPoints{${1:formule1 § formule2 § ...}}"
    },
    "ImagesPoints[]{}": {
      "snippet": "ImagesPoints[${2:clés}]{${1:formule1 § formule2 § ...}}"
    },
    "ImagesPoints*{}": {
      "snippet": "ImagesPoints*{${1:formule1 § formule2 § ...}}"
    },
    "ImagesPoints*[]{}": {
      "snippet": "ImagesPoints*[${2:clés}]{${1:formule1 § formule2 § ...}}"
    },
    "TracerSegments{}": {
      "snippet": "TracerSegments{${1:ptA/ptB ptC/ptD ...}}"
    },
    "TracerSegments<>{}": {
      "snippet": "TracerSegments<${2:options TikZ}>{${1:ptA/ptB ptC/ptD ...}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "TracerSegments[]{}": {
      "snippet": "TracerSegments[${2:clés}]{${1:ptA/ptB ptC/ptD ...}}"
    },
    "TracerSegments[]<>{}": {
      "snippet": "TracerSegments[${2:clés}]<${3:options TikZ}>{${1:ptA/ptB ptC/ptD ...}}",
      "keyvalindex": 0,
      "keyvalpos": 1
    },
    "TransfPosTikzDD{}": {
      "snippet": "TransfPosTikzDD{${1:arg}}"
    },
    "TwoDimLabel": {},
    "TwoDimDecalLabel": {},
    "TwoDimStylePt": {},
    "TwoDimtaillePt": {},
    "TwoDimCoul": {},
    "TwoDimFonte": {},
    "TracSegCap": {},
    "TracSegJoin": {},
    "TracSegCoul": {}
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "Cap=${1:type}",
      "Join=${1:type}",
      "Couleurs=${1:liste de couleurs}",
      "Cycle",
      "Suite"
    ]
  ]
}