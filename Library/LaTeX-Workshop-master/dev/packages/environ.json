{
  "includes": {
    "trimspaces": []
  },
  "macros": {
    "NewEnviron{}{}": {
      "snippet": "NewEnviron{${1:envname}}{${2:definition}}"
    },
    "NewEnviron{}[]{}": {
      "snippet": "NewEnviron{${1:envname}}[${2:args}]{${3:definition}}"
    },
    "NewEnviron{}[][]{}": {
      "snippet": "NewEnviron{${1:envname}}[${2:args}][${3:default}]{${4:definition}}"
    },
    "NewEnviron{}[][]{}[]": {
      "snippet": "NewEnviron{${1:envname}}[${2:args}][${3:default}]{${4:definition}}[${5:final code}]"
    },
    "RenewEnviron{}{}": {
      "snippet": "RenewEnviron{${1:envname}}{${2:definition}}"
    },
    "RenewEnviron{}[]{}": {
      "snippet": "RenewEnviron{${1:envname}}[${2:args}]{${3:definition}}"
    },
    "RenewEnviron{}[][]{}": {
      "snippet": "RenewEnviron{${1:envname}}[${2:args}][${3:default}]{${4:definition}}"
    },
    "RenewEnviron{}[][]{}[]": {
      "snippet": "RenewEnviron{${1:envname}}[${2:args}][${3:default}]{${4:definition}}[${5:final code}]"
    },
    "BODY": {},
    "environfinalcode{}": {
      "snippet": "environfinalcode{${1:code}}"
    },
    "environbodyname{}": {
      "snippet": "environbodyname{${1:cmd}}"
    }
  },
  "envs": {},
  "options": [],
  "keyvals": []
