{
  "includes": {},
  "macros": {
    "tikzmark{}": {
      "snippet": "tikzmark{${1:name}}"
    },
    "tikzmark[]{}": {
      "snippet": "tikzmark[${2:drawing command}]{${1:name}}"
    },
    "pgfmark{}": {
      "snippet": "pgfmark{${1:name}}"
    },
    "iftikzmark{}{}{}": {
      "snippet": "iftikzmark{${1:name}}{${2:true}}{${3:false}}"
    },
    "iftikzmarkexists{}": {
      "snippet": "iftikzmarkexists{${1:name}}"
    },
    "iftikzmarkoncurrentpage{}": {
      "snippet": "iftikzmarkoncurrentpage{${1:name}}"
    },
    "iftikzmarkonpage{}{}": {
      "snippet": "iftikzmarkonpage{${1:name}}{${2:page}}"
    },
    "tikzmarknode{}{}": {
      "snippet": "tikzmarknode{${1:name}}{${2:contents}}"
    },
    "tikzmarknode[]{}{}": {
      "snippet": "tikzmarknode[${3:options}]{${1:name}}{${2:contents}}"
    },
    "subnode{}{}": {
      "snippet": "subnode{${1:name}}{${2:content}}"
    },
    "subnode[]{}{}": {
      "snippet": "subnode[${3:options}]{${1:name}}{${2:content}}"
    },
    "SaveNode{}": {
      "snippet": "SaveNode{${1:name}}"
    },
    "SaveNode[]{}": {
      "snippet": "SaveNode[${2:group name}]{${1:name}}"
    },
    "usetikzmarklibrary{}": {
      "snippet": "usetikzmarklibrary{${1:library}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "tikzmarkmath": {},
    "tikzmarkmath[]": {
      "snippet": "tikzmarkmath[${1:prefix}]"
    },
    "endtikzmarkmath": {},
    "thetikzmarkequation": {},
    "StartHighlighting": {},
    "StartHighlighting[]": {
      "snippet": "StartHighlighting[${1:options}]"
    },
    "StopHighlighting": {},
    "Highlight{}": {
      "snippet": "Highlight{${1:text}}"
    },
    "Highlight[]{}": {
      "snippet": "Highlight[${2:options}]{${1:text}}"
    },
    "savepointas{}{}{}{}": {
      "snippet": "savepointas{${1:arg1}}{${2:arg2}}{${3:arg3}}{${4:arg4}}"
    },
    "savepicturepage{}{}": {
      "snippet": "savepicturepage{${1:arg1}}{${2:arg2}}"
    },
    "tikzmarkalias{}{}": {
      "snippet": "tikzmarkalias{${1:arg1}}{${2:arg2}}"
    }
  },
  "envs": {
    "tikzmarkmath": {},
    "tikzmarkmath[]": {
      "name": "tikzmarkmath",
      "snippet": "[${1:prefix}]"
    }
  },
  "options": [],
  "keyvals": [
    [
      "listings",
      "ams",
      "highlighting"
    ]
  ]
}