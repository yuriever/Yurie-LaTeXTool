{
  "includes": {
    "enumitem": [],
    "pgfkeys": [],
    "conditionals": [],
    "microtype": []
  },
  "macros": {
    "epigraph{}": {
      "snippet": "epigraph{${1:text}}"
    },
    "epigraph[]{}": {
      "snippet": "epigraph[${2:keyvals}]{${1:text}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "qitem{}": {
      "snippet": "qitem{${1:text}}"
    },
    "qitem[]{}": {
      "snippet": "qitem[${2:keyvals}]{${1:text}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "expblank{}": {
      "snippet": "expblank{${1:arg}}"
    },
    "expgiven{}": {
      "snippet": "expgiven{${1:arg}}"
    },
    "expnil{}": {
      "snippet": "expnil{${1:arg}}"
    },
    "beforeepigraphskip": {},
    "afterepigraphskip": {},
    "epigraphtextindent": {},
    "epigraphauthorsourceindent": {},
    "epigraphtextwidth": {},
    "epigraphstyle": {},
    "epigraphdash": {},
    "epigraphquotefont": {},
    "epigraphtranslationfont": {}
  },
  "envs": {
    "epigraphs": {}
  },
  "options": [],
  "keyvals": [
    [
      "author={${1:author}}",
      "source={${1:source}}",
      "translation={${1:translation text}}",
      "etc={${1:text}}",
      "after skip=",
      "before skip=",
      "author and source indent=",
      "text indent=",
      "width=",
      "style=${1:font commands}",
      "quote style=${1:font commands}",
      "translation style=${1:font commands}",
      "dash={${1:dash code}}"
    ]
  ]
}