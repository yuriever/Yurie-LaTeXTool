{
  "includes": {
    "kvoptions": [],
    "tikz": [],
    "tikzlibraryexternal": [],
    "standalone": [],
    "hep-plot": [
      "plot",
      "plot=true"
    ],
    "hep-feynman": [
      "feynman",
      "feynman=true"
    ]
  },
  "macros": {
    "graphicpath{}": {
      "snippet": "graphicpath{${1:path}}"
    },
    "includetikz{}": {
      "snippet": "includetikz{${1:file}}"
    },
    "includetikz[]{}": {
      "snippet": "includetikz[${2:width}]{${1:file}}"
    },
    "includetikz*{}": {
      "snippet": "includetikz*{${1:file}}"
    },
    "includetikz*[]{}": {
      "snippet": "includetikz*[${2:width}]{${1:file}}"
    },
    "includefeynman{}": {
      "snippet": "includefeynman{${1:file}}"
    }
  },
  "envs": {},
  "options": [
    "plot",
    "feynman"
  ],
  "keyvals": []
}