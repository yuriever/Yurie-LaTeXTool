{
  "includes": {},
  "macros": {
    "markversion{}": {
      "snippet": "markversion{${1:version}}"
    },
    "includeversion{}": {
      "snippet": "includeversion{${1:version}}"
    },
    "excludeversion{}": {
      "snippet": "excludeversion{${1:version}}"
    },
    "processifversion{}{}": {
      "snippet": "processifversion{${1:version}}{${2:code}}"
    },
    "includeversionnogroup{}": {
      "snippet": "includeversionnogroup{${1:version}}",
      "option": "nogroup"
    },
    "versionmessage{}{}": {
      "snippet": "versionmessage{${1:version}}{${2:type}}"
    },
    "beginmarkversion": {},
    "endmarkversion": {}
  },
  "envs": {
    "comment": {}
  },
  "options": [
    "nogroup",
    "tracing"
  ],
  "keyvals": []
}