{
  "includes": {
    "mathtools": [],
    "autoaligne": [],
    "icomma": [],
    "pgfplots": [],
    "tikzlibraryshapes.misc": []
  },
  "macros": {
    "LGS{}": {
      "snippet": "LGS{${1:Gleichungen}}"
    },
    "ehoch{}": {
      "snippet": "ehoch{${1:Exponent}}"
    },
    "diff{}": {
      "snippet": "diff{${1:Variable}}"
    },
    "Pkt{}{}": {
      "snippet": "Pkt{${1:x}}{${2:y}}"
    },
    "Pkt[]{}{}": {
      "snippet": "Pkt[${3:Name}]{${1:x}}{${2:y}}"
    },
    "PktR{}{}{}": {
      "snippet": "PktR{${1:x}}{${2:y}}{${3:z}}"
    },
    "Pkt[]{}{}{}": {
      "snippet": "Pkt[${4:Name}]{${1:x}}{${2:y}}{${3:z}}"
    },
    "Vek{}{}{}": {
      "snippet": "Vek{${1:x}}{${2:y}}{${3:z}}"
    },
    "VekBr{}{}{}": {
      "snippet": "VekBr{${1:x}}{${2:y}}{${3:z}}"
    },
    "GTRY{}": {
      "snippet": "GTRY{${1:index}}"
    },
    "GTRY{}[]": {
      "snippet": "GTRY{${1:index}}[${2:term}]"
    },
    "Rechenkaestchen{}{}": {
      "snippet": "Rechenkaestchen{${1:Breite}}{${2:Höhe}}"
    },
    "Rechenkaestchen[]{}{}": {
      "snippet": "Rechenkaestchen[${3:Kästchengröße}]{${1:Breite}}{${2:Höhe}}"
    }
  },
  "envs": {
    "Kosy": {},
    "Kosy[]": {
      "name": "Kosy",
      "snippet": "[${1:pgfplots options}]"
    }
  },
  "options": [
    "ohne-icomma"
  ],
  "keyvals": []
}