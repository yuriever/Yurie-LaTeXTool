{
  "includes": {
    "l3keys2e": [],
    "siunitx": [],
    "tikz": [],
    "tikzlibrarycalc": []
  },
  "macros": {
    "ENsetup{}": {
      "snippet": "ENsetup{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "ENsetup[]{}": {
      "snippet": "ENsetup[${2:module}]{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 1
    },
    "ENcurve{}": {
      "snippet": "ENcurve{${1:level1,level2,level3,...}}"
    },
    "ENcurve[]{}": {
      "snippet": "ENcurve[${2:options}]{${1:level1,level2,level3,...}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "ShowNiveaus": {},
    "ShowNiveaus[]": {
      "snippet": "ShowNiveaus[${1:options}]",
      "keyvalindex": 2,
      "keyvalpos": 0
    },
    "ShowGain": {},
    "ShowGain[]": {
      "snippet": "ShowGain[${1:options}]",
      "keyvalindex": 3,
      "keyvalpos": 0
    },
    "ShowEa": {},
    "ShowEa[]": {
      "snippet": "ShowEa[${1:options}]",
      "keyvalindex": 4,
      "keyvalpos": 0
    },
    "MakeOrigin": {}
  },
  "envs": {
    "endiagram": {},
    "endiagram[]": {
      "name": "endiagram",
      "snippet": "[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
  "options": [],
  "keyvals": [
    [
      "unit=",
      "scale=${1:factor}",
      "offset=${1:integer}",
      "r-offset=${1:integer}",
      "l-offset=${1:integer}",
      "axes=",
      "x-axis={${1:TikZ keys}}",
      "y-axis={${1:TikZ keys}}",
      "x-label=",
      "y-label=",
      "x-label-pos=${1:value}",
      "y-label-pos=${1:value}",
      "x-label-offset=",
      "y-label-offset=",
      "x-label-angle=${1:degrees}",
      "y-label-angle=${1:degrees}",
      "x-label-text=${1:text}",
      "y-label-text=${1:text}",
      "debug",
      "draft",
      "final",
      "tikz={${1:TikZ keys}}",
      "ticks=",
      "ticks-step=${1:number}",
      "energy-unit=${1:unit}",
      "energy-step=${1:number}",
      "energy-zero=${1:number}",
      "energy-unit-separator=${1:separator}",
      "energy-round=${1:number}",
      "calculate"
    ],
    [
      "step=${1:integer}",
      "looseness=${1:factor}",
      "minima",
      "tikz={${1:TikZ keys}}"
    ],
    [
      "length=${1:number}",
      "shift=${1:number}",
      "tikz={${1:TikZ keys}}",
      "niveau=${1:id}"
    ],
    [
      "tikz={${1:TikZ keys}}",
      "connect={${1:TikZ keys}}",
      "connect-from-line",
      "offset=${1:number}",
      "label",
      "label=${1:text}",
      "label-side=",
      "label-pos=${1:value}",
      "label-tikz={${1:TikZ keys}}"
    ],
    [
      "max=",
      "from={(${1:x1,y1}) to (${2:x2,y2})}",
      "tikz={${1:TikZ keys}}",
      "connect={${1:TikZ keys}}",
      "label",
      "label=${1:text}",
      "label-side=",
      "label-pos=${1:value}",
      "label-tikz={${1:TikZ keys}}"
    ]
  ]
}