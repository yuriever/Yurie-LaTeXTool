{
  "includes": {
    "xtemplate": [],
    "l3keys2e": [],
    "class-ctexbook": [],
    "xeCJK": [],
    "amsmath": [],
    "unicode-math": [],
    "geometry": [],
    "fancyhdr": [],
    "footmisc": [],
    "ntheorem": [],
    "graphicx": [],
    "longtable": [],
    "caption": [],
    "xcolor": [],
    "pifont": [],
    "natbib": [],
    "biblatex": [],
    "hyperref": []
  },
  "macros": {
    "fdusetup{}": {
      "snippet": "fdusetup{${1:keyvals}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "makecoveri": {},
    "makecoverii": {},
    "makecoveriii": {},
    "newtheorem[]{}{}": {
      "snippet": "newtheorem[${3:options}]{${1:envname}}{${2:title}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "newtheorem*{}{}": {
      "snippet": "newtheorem*{${1:envname}}{${2:title}}"
    },
    "newtheorem*[]{}{}": {
      "snippet": "newtheorem*[${3:options}]{${1:envname}}{${2:title}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "DeclareCoverTemplate{}{}": {
      "snippet": "DeclareCoverTemplate{${1:arg1}}{${2:arg2}}"
    },
    "theoremsymbol{}": {
      "snippet": "theoremsymbol{${1:symbol}}"
    }
  },
  "envs": {
    "abstract*": {},
    "notation": {},
    "notation[]": {
      "name": "notation",
      "snippet": "[${1:cols}]"
    },
    "acknowledgements": {},
    "axiom": {},
    "axiom[]": {
      "name": "axiom",
      "snippet": "[${1:subheading}]"
    },
    "corollary": {},
    "corollary[]": {
      "name": "corollary",
      "snippet": "[${1:subheading}]"
    },
    "definition": {},
    "definition[]": {
      "name": "definition",
      "snippet": "[${1:subheading}]"
    },
    "example": {},
    "example[]": {
      "name": "example",
      "snippet": "[${1:subheading}]"
    },
    "lemma": {},
    "lemma[]": {
      "name": "lemma",
      "snippet": "[${1:subheading}]"
    },
    "proof": {},
    "proof[]": {
      "name": "proof",
      "snippet": "[${1:subheading}]"
    },
    "theorem": {},
    "theorem[]": {
      "name": "theorem",
      "snippet": "[${1:subheading}]"
    }
  },
  "options": [
    "type=",
    "oneside",
    "twoside",
    "draft",
    "config=${1:file}"
  ],
  "keyvals": [
    [
      "style={${1:options}}",
      "style/font=",
      "style/cjk-font=",
      "style/font-size=",
      "style/fullwidth-stop=",
      "style/footnote-style=",
      "style/hyperlink=",
      "style/hyperlink-color=",
      "style/bib-backend=",
      "style/bib-style=",
      "style/cite-style=${1:style}",
      "style/bib-resource={${1:bib files}}",
      "style/logo=${1:imagefile}",
      "style/logo-size=",
      "style/logo-size={${1:width,height}}",
      "style/auto-make-cover",
      "style/declaration-page=${1:PDF file}",
      "info={${1:options}}",
      "info/degree=",
      "info/title={${1:title in Chinese}}",
      "info/title*={${1:title in English}}",
      "info/author={${1:name in Chinese}}",
      "info/author*={${1:name in English}}",
      "info/supervisor=${1:name}",
      "info/department=${1:name}",
      "info/major=${1:name}",
      "info/student-id=${1:number}",
      "info/school-id=${1:number}",
      "info/date=${1:date}",
      "info/secret-level=",
      "info/secret-year=${1:year}",
      "info/instructors={${1:member1,member2,...}}",
      "info/keywords={${1:keywords in Chinese}}",
      "info/keywords*={${1:keywords in English}}",
      "info/clc={${1:classification codes}}",
      "info/jel={${1:classification codes}}",
      "theorem={${1:options}}",
      "theorem/style=",
      "theorem/header-font=${1:font commands}",
      "theorem/body-font=${1:font commands}",
      "theorem/qed=${1:symbol}",
      "theorem/counter=${1:counter}"
    ],
    [
      "style=",
      "header-font=${1:font commands}",
      "body-font=${1:font commands}",
      "qed=${1:symbol}",
      "counter=${1:counter}"
    ]
  ]
}