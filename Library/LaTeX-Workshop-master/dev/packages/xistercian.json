      "sv=",
      "stroke-h=",
      "sh=",
      "stroke-du=",
      "sdu=",
      "stroke-dd=",
      "sdd=",
      "strokes=",
      "s=",
      "fraction=${1:fraction}",
      "fr=${1:fraction}",
      "orientation=",
      "o=",
      "horizontal",
      "vertical",
      "alternate-5=${1:choice}",
      "5=${1:choice}",
      "alternate-6=${1:choice}",
      "6=${1:choice}",
      "alternate-9=${1:choice}",
      "9=${1:choice}",
      "font={${1:font setup}}",
      "f={${1:font setup}}",
      "%cisterciankey",
      "redraw={${1:feature list}}",
      "redraw +={${1:feature list}}",
      "redraw",
      "redraw!",
      "r={${1:feature list}}",
      "r +={${1:feature list}}",
      "r",
      "r!"
    ]
  ]
