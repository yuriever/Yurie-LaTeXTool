  "macros": {
    "cistercian{}": {
      "snippet": "cistercian{${1:counter}}"
    },
    "cisterciannum{}": {
      "snippet": "cisterciannum{${1:integer}}"
    },
    "cisterciannumE{}": {
      "snippet": "cisterciannumE{${1:integer expression}}"
    },
    "cisterciansetup{}": {
      "snippet": "cisterciansetup{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "cistercianredraw": {},
    "cistercianredrawlazy": {},
    "cistercianstyle{}{}": {
      "snippet": "cistercianstyle{${1:keyname}}{${2:options}}",
      "keyvalindex": 0,
      "keyvalpos": 1
    },
    "cistercianstyle*{}{}": {
      "snippet": "cistercianstyle*{${1:keyname}}{${2:options}}",
      "keyvalindex": 0,
      "keyvalpos": 1
    }
  },
  "envs": {},
