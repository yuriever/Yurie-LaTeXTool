{
  "includes": {
    "xkeyval": [],
    "ifpdf": [],
    "ifxetex": [],
    "eforms": []
  },
  "macros": {
    "rtpara{}{}": {
      "snippet": "rtpara{${1:name}}{${2:richtext paragraph}}"
    },
    "rtpara[]{}{}": {
      "snippet": "rtpara[${3:options}]{${1:name}}{${2:richtext paragraph}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "span{}{}": {
      "snippet": "span{${1:options}}{${2:richtext string}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "useRV{}": {
      "snippet": "useRV{${1:name}}"
    },
    "useV{}": {
      "snippet": "useV{${1:name}}"
    },
    "sub{}": {
      "snippet": "sub{${1:text}}"
    },
    "sup{}": {
      "snippet": "sup{${1:text}}"
    },
    "spc": {},
    "br": {},
    "RV{}": {
      "snippet": "RV{${1:rich value}}"
    },
    "DS{}": {
      "snippet": "DS{${1:default style}}"
    },
    "useDefaultDS": {},
    "setDefaultStyle{}{}": {
      "snippet": "setDefaultStyle{${1:name}}{${2:options}}",
      "keyvalindex": 1,
      "keyvalpos": 1
    },
    "useDS{}": {
      "snippet": "useDS{${1:name}}"
    },
    "setRVVContent{}{{}{}": {
      "snippet": "setRVVContent{${1:name}}{{${2:name1}}{${3:name2}...}}"
    },
    "useRVContent{}": {
      "snippet": "useRVContent{${1:name}}"
    },
    "useVContent{}": {
      "snippet": "useVContent{${1:name}}"
    },
    "displayRV{}": {
      "snippet": "displayRV{${1:name}}"
    },
    "displayV{}": {
      "snippet": "displayV{${1:name}}"
    },
    "contName": {},
    "displayRtParaName": {},
    "makePDFSp": {},
    "makeTeXSp": {},
    "makeTeXSpPrnt": {},
    "resetRtFontKeys": {},
    "rtpdfSPDef": {},
    "rtpdfSPDefPrnt": {},
    "rvorvstring{}{}": {
      "snippet": "rvorvstring{${1:arg1}}{${2:arg2}}"
    }
  },
  "envs": {
    "displayRtPara{}": {
      "name": "displayRtPara",
      "snippet": "{${1:name}}"
    },
    "displayRtPara*{}": {
      "name": "displayRtPara*",
      "snippet": "{${1:name}}"
    }
  },
  "options": [],
  "keyvals": [
    [
      "font=${1:font name}",
      "size=${1:number}",
      "raise=${1:number}",
      "ulstyle=",
      "style={${1:bold, italic, and/or strikeit}}",
      "color=${1:rrggbb}",
      "url={${1:URL}}",
      "raw=${1:CSS2 markup}",
      "halign=",
      "valign=",
      "margleft=${1:number}",
      "margright=${1:number}",
      "indent=",
      "indentby=${1:number}",
      "margtop=${1:number}",
      "margbottom=${1:number}",
      "linespacing=",
      "lineheight=${1:number}"
    ],
    [
      "font=${1:font name}",
      "size=${1:number}",
      "raise=${1:number}",
      "ulstyle=",
      "style={${1:bold, italic, and/or strikeit}}",
      "color=${1:rrggbb}",
      "url={${1:URL}}",
      "raw=${1:CSS2 markup}"
    ]
  ]
}