      "snippet": "splicelist{${1:arg}}"
    }
  },
  "envs": {
    "polyhedron{}": {
      "name": "polyhedron",
      "snippet": "{${1:keyvals}}",
      "keyvalindex": 6,
      "keyvalpos": 0
    }
  },
  "options": [],
  "keyvals": [
    [
      "x={${1:num1,num2}}",
      "y={${1:num1,num2}}",
      "z={${1:num1,num2}}",
      "xlabel=${1:text}",
      "ylabel=${1:text}",
      "zlabel=${1:text}",
      "labels="
