      "snippet": "cone{${1:keyvals}}",
      "keyvalindex": 3,
      "keyvalpos": 0
    },
    "polygon{}": {
      "snippet": "polygon{${1:keyvals}}",
      "keyvalindex": 4,
      "keyvalpos": 0
    },
    "halfspace{}": {
      "snippet": "halfspace{${1:keyvals}}",
      "keyvalindex": 5,
