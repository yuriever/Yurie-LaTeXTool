{
  "includes": {
    "tikzpagenodes": []
  },
  "macros": {
    "snaptodo{}": {
      "snippet": "snaptodo{${1:text}}"
    },
    "snaptodo[]{}": {
      "snippet": "snaptodo[${2:options}]{${1:text}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "snaptodoset{}": {
      "snippet": "snaptodoset{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "call chain/.style={${1:TikZ styles}}",
      "margin block/.style={${1:TikZ styles}}",
      "block sep=",
      "chain sep=",
      "chain bias=",
      "block rise="
    ]
  ]
}