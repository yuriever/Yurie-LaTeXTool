  "macros": {
    "smartdiagram[]{}": {
      "snippet": "smartdiagram[${2:type}]{${1:list of items}}"
    },
    "smartdiagramanimated[]{}": {
      "snippet": "smartdiagramanimated[${2:type}]{${1:list of items}}"
    },
    "usesmartdiagramlibrary{}": {
      "snippet": "usesmartdiagramlibrary{${1:library}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "smartdiagramadd[]{}{}": {
      "snippet": "smartdiagramadd[${3:type}]{${1:list of items}}{${2:list of additions}}"
    },
    "smartdiagramconnect{}{}": {
      "snippet": "smartdiagramconnect{${1:arrow options}}{${2:start module/end module}}"
    },
