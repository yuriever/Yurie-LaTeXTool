{
  "includes": {
    "multido": [],
    "amsmath": [],
    "graphicx": [],
    "pgf": [],
    "hyperref": []
  },
  "macros": {
    "title[]{}": {
      "snippet": "title[${2:short title}]{${1:text}}"
    },
    "author[]{}": {
      "snippet": "author[${2:short author}]{${1:names}}"
    },
    "slidesmag{}": {
      "snippet": "slidesmag{${1:factor}}"
    },
    "slidestyle{}": {
      "snippet": "slidestyle{${1:style}}"
    },
    "fromslide{}{}": {
      "snippet": "fromslide{${1:slide number}}{${2:material}}"
    },
    "fromslide*{}{}": {
      "snippet": "fromslide*{${1:slide number}}{${2:material}}"
    },
    "toslide{}{}": {
      "snippet": "toslide{${1:slide number}}{${2:material}}"
    },
    "toslide*{}{}": {
      "snippet": "toslide*{${1:slide number}}{${2:material}}"
    },
    "onlyslide{}{}": {
      "snippet": "onlyslide{${1:slide number}}{${2:material}}"
    },
    "onlyslide*{}{}": {
      "snippet": "onlyslide*{${1:slide number}}{${2:material}}"
    },
    "tableofcontents[]": {
      "snippet": "tableofcontents[${1:<${2:fromsec}>.<${3:fromsubsec}>-<${4:tosec}>.<${5:tosubsec}>}]"
    },
    "slidewidth": {},
    "slideheight": {},
    "theslide": {},
    "thesubslide": {},
    "theslidelabel": {},
    "slidesadjust{}{}": {
      "snippet": "slidesadjust{${1:hadjust}}{${2:vadjust}}"
    }
  },
  "envs": {
    "slide{}": {
      "name": "slide",
      "snippet": "{${1:title}}"
    },
    "slide[]{}": {
      "name": "slide",
      "snippet": "[${2:style}]{${1:title}}"
    },
    "notes": {},
    "multislide{}{}": {
      "name": "multislide",
      "snippet": "{${1:sub-slides}}{${2:title}}"
    },
    "multislide[]{}{}": {
      "name": "multislide",
      "snippet": "[${3:style}]{${1:sub-slides}}{${2:title}}"
    }
  },
  "options": [
    "notes",
    "slides",
    "screen",
    "rotate",
    "norotate",
    "a4paper",
    "a5paper",
    "b5paper",
    "letterpaper",
    "legalpaper",
    "executivepaper",
    "landscape",
    "10pt",
    "11pt",
    "12pt",
    "oneside",
    "twoside",
    "draft",
    "final",
    "titlepage",
    "notitlepage",
    "onecolumn",
    "twocolumn",
    "leqno",
    "fleqn",
    "openbib"
  ],
  "keyvals": []
}