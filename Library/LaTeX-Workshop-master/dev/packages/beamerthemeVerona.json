{
  "includes": {
    "tikz": [],
    "tcolorbox": [],
    "tcolorboxlibraryskins": []
  },
  "macros": {
    "lecturename": {},
    "autorecitazione": {},
    "structureA": {},
    "structureB": {},
    "oldtitlegraphic": {},
    "titlegraphic{}{}": {
      "snippet": "titlegraphic{${1:imagefile}}{${2:TikZ options}}"
    },
    "titlegraphic[]{}{}": {
      "snippet": "titlegraphic[${3:options}]{${1:imagefile}}{${2:TikZ options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "sidegraphics{}{}": {
      "snippet": "sidegraphics{${1:imagefile}}{${2:TikZ options}}",
      "option": "sidebar"
    },
    "sidegraphics[]{}{}": {
      "snippet": "sidegraphics[${3:options}]{${1:imagefile}}{${2:TikZ options}}",
      "option": "sidebar",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "sidegraphics<>{}{}": {
      "snippet": "sidegraphics<${3:overlay specification}>{${1:imagefile}}{${2:TikZ options}}",
      "option": "sidebar"
    },
    "sidegraphics<>[]{}{}": {
      "snippet": "sidegraphics<${4:overlay specification}>[${3:options}]{${1:imagefile}}{${2:TikZ options}}",
      "option": "sidebar",
      "keyvalindex": 0,
      "keyvalpos": 1
    },
    "lectureinfoot": {},
    "frametitlesidebar": {},
    "mail{}": {
      "snippet": "mail{${1:address}}"
    },
    "datelecture": {}
  },
  "envs": {
    "citazione": {},
    "citazione<>": {
      "name": "citazione",
      "snippet": "<${1:action specification}>"
    },
    "citazione[]": {
      "name": "citazione",
      "snippet": "[${1:author}]"
    },
    "citazione<>[]": {
      "name": "citazione",
      "snippet": "<${2:action specification}>[${1:author}]"
    },
    "quotation[]": {
      "name": "quotation",
      "snippet": "[${1:author}]"
    },
    "quotation<>[]": {
      "name": "quotation",
      "snippet": "<${2:action specification}>[${1:author}]"
    }
  },
  "options": [],
  "keyvals": [
    [
      "alt={${1:alt text}}",
      "bb=${1:llx lly urx ury}",
      "bbllx=",
      "bblly=",
      "bburx=",
      "bbury=",
      "natwidth=",
      "natheight=",
      "hiresbb",
      "pagebox=",
      "viewport=${1:llx lly urx ury}",
      "trim=${1:llx lly urx ury}",
      "angle=${1:degrees}",
      "origin=",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}",
      "clip",
      "draft",
      "type=${1:file type}",
      "ext=${1:file extension}",
      "read=${1:read-file extension}",
      "command=",
      "quiet",
      "page=${1:page number}",
      "interpolate",
      "decodearray={${1:color array}}"
    ]
  ]
}