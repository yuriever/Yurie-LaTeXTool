{
  "includes": {},
  "macros": {
    "newlist{}{}{}": {
      "snippet": "newlist{${1:name}}{${2:type}}{${3:max-depth}}"
    },
    "renewlist{}{}{}": {
      "snippet": "renewlist{${1:name}}{${2:type}}{${3:max-depth}}"
    },
    "setlist{}": {
      "snippet": "setlist{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "setlist*{}": {
      "snippet": "setlist*{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "setlist[]{}": {
      "snippet": "setlist[${2:names, levels}]{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 1
    },
    "setlist*[]{}": {
      "snippet": "setlist*[${2:names, levels}]{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 1
    },
    "restartlist{}": {
      "snippet": "restartlist{${1:list name}}"
    },
    "EnumitemId": {},
    "SetLabelAlign{}{}": {
      "snippet": "SetLabelAlign{${1:value}}{${2:commands}}"
    },
    "labelindent": {},
    "SetEnumerateShortLabel{}{}": {
      "snippet": "SetEnumerateShortLabel{${1:key}}{${2:replacement}}"
    },
    "setlistdepth{}": {
      "snippet": "setlistdepth{${1:integer}}"
    },
    "AddEnumerateCounter{}{}{}": {
      "snippet": "AddEnumerateCounter{${1:LaTeX-cmd}}{${2:internal-cmd}}{${3:widest label}}"
    },
    "AddEnumerateCounter*{}{}{}": {
      "snippet": "AddEnumerateCounter*{${1:LaTeX-cmd}}{${2:internal-cmd}}{${3:integer}}"
    },
    "SetEnumitemKey{}{}": {
      "snippet": "SetEnumitemKey{${1:key}}{${2:replacement options}}"
    },
    "SetEnumitemValue{}{}{}": {
      "snippet": "SetEnumitemValue{${1:key}}{${2:string-value}}{${3:replacement}}"
    },
    "SetEnumitemSize{}{}": {
      "snippet": "SetEnumitemSize{${1:name}}{${2:selector}}"
    },
    "DrawEnumitemLabel": {}
  },
  "envs": {
    "enumerate*": {
      "option": "inline"
    },
    "enumerate*[]": {
      "name": "enumerate*",
      "snippet": "[${1:options}]",
      "option": "inline",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "itemize*": {
      "option": "inline"
    },
    "itemize*[]": {
      "name": "itemize*",
      "snippet": "[${1:options}]",
      "option": "inline",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "description*": {
      "option": "inline"
    },
    "description*[]": {
      "name": "description*",
      "snippet": "[${1:options}]",
      "option": "inline",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "enumerate[]": {
      "name": "enumerate",
      "snippet": "[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "itemize[]": {
      "name": "itemize",
      "snippet": "[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "description[]": {
      "name": "description",
      "snippet": "[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
  "options": [
    "shortlabels",
    "inline",
    "sizes",
    "ignoredisplayed",
    "includedisplayed",
    "series=override",
    "loadonly"
