{
  "includes": {
    "grruby": []
  },
  "macros": {
    "readhanja{}": {
      "snippet": "readhanja{${1:hanja}}"
    },
    "readhanja{}[]": {
      "snippet": "readhanja{${1:hanja}}[${2:number}]"
    },
    "readhanjaword{}": {
      "snippet": "readhanjaword{${1:hanja}}"
    },
    "rwhanja{}": {
      "snippet": "rwhanja{${1:hanja}}"
    },
    "AssignReading{}{}": {
      "snippet": "AssignReading{${1:hanja}}{${2:reading}}"
    },
    "rpSetReading{}": {
      "snippet": "rpSetReading{${1:hanja=reading}}"
    },
    "grrwhanja{}": {
      "snippet": "grrwhanja{${1:hanja}}"
    },
    "rwhanjachar{}": {
      "snippet": "rwhanjachar{${1:hanja}}"
    },
    "rpRead{}": {
      "snippet": "rpRead{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "rpRead{}[]": {
      "snippet": "rpRead{${1:options}}[${2:hanja=reading}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "viewCodePoint{}": {
      "snippet": "viewCodePoint{${1:unicode char}}"
    },
    "viewCodePoint*{}": {
      "snippet": "viewCodePoint*{${1:unicode char}}"
    },
    "showReadings{}": {
      "snippet": "showReadings{${1:hanja}}"
    },
    "hnjahnglpkgdate": {},
    "hnjahnglpkgversion": {}
  },
  "envs": {},
  "options": [
    "grruby=false"
  ],
  "keyvals": [
    [
      "comma",
      "para",
      "at",
      "period"
    ]
  ]
}