      "explode=${1:number}",
      "sum=",
      "change direction",
      "before number={${1:code}}",
      "after number={${1:code}}",
      "hide number",
      "scale font",
      "text=",
      "style={${1:TikZ options}}",
      "polar",
      "square",
      "cloud"
    ]
  ]
