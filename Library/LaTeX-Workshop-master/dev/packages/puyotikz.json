{
  "includes": {
    "pythontex": [],
    "tikz": [],
    "keyval": []
  },
  "macros": {
    "puyosmallscale": {},
    "puyobigscale": {},
    "puyoboard{}{}": {
      "snippet": "puyoboard{${1:board string}}{${2:next puyos string}}"
    },
    "puyoboard[]{}{}": {
      "snippet": "puyoboard[${3:options}]{${1:board string}}{${2:next puyos string}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "puyomarker{}": {
      "snippet": "puyomarker{${1:marker string}}"
    },
    "puyogrid{}": {
      "snippet": "puyogrid{${1:board string}}"
    },
    "puyogrid[]{}": {
      "snippet": "puyogrid[${2:options}]{${1:board string}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "puyocolor{}{}": {
      "snippet": "puyocolor{${1:name}}{${2:r,g,b}}"
    }
  },
  "envs": {
    "puyotikz": {},
    "puyotikz[]": {
      "name": "puyotikz",
      "snippet": "[${1:scale}]"
    }
  },
  "options": [],
  "keyvals": [
    [
      "ncols=${1:number}",
      "nrows=${1:number}",
      "nhidrows=${1:number}",
      "showlabels="
    ]
  ]
}