    ],
    "lastpage": [
      "template",
      "template=9x6"
    ],
    "fancyhdr": [
      "template",
      "template=9x6"
    ]
  },
  "macros": {
    "pptLeft{}": {
      "snippet": "pptLeft{${1:text}}",
      "option": "template=9x6"
    },
    "pptRight{}": {
      "snippet": "pptRight{${1:text}}",
      "option": "template=9x6"
    },
    "pptBanner{}": {
      "snippet": "pptBanner{${1:text}}"
    },
    "pptBanner[]{}": {
      "snippet": "pptBanner[${2:color}]{${1:text}}"
    },
    "pptChapter{}": {
      "snippet": "pptChapter{${1:title}}"
    },
    "pptChapter[]{}": {
      "snippet": "pptChapter[${2:short title}]{${1:title}}"
    },
    "pptSection{}": {
      "snippet": "pptSection{${1:title}}"
    },
    "pptSection[]{}": {
      "snippet": "pptSection[${2:short title}]{${1:title}}"
    },
    "pptHeader{}": {
      "snippet": "pptHeader{${1:text}}"
    },
    "pptTitle{}{}": {
      "snippet": "pptTitle{${1:title}}{${2:subtitle}}"
    },
    "pptTOC": {},
    "pptToc": {},
    "pptToc[]": {
      "snippet": "pptToc[${1:text}]"
    },
    "param": {},
    "pptQuote{}{}{}": {
      "snippet": "pptQuote{${1:imagefile}}{${2:quote}}{${3:author}}"
    },
    "pptQuote[]{}{}{}": {
      "snippet": "pptQuote[${4:name}]{${1:imagefile}}{${2:quote}}{${3:author}}"
    },
    "pptPic{}{}": {
      "snippet": "pptPic{${1:width factor}}{${2:imagefile}}"
    },
    "pptPin{}": {
      "snippet": "pptPin{${1:text}}"
    },
    "pptPin[]{}": {
      "snippet": "pptPin[${2:right|left}]{${1:text}}"
    },
    "pptThought{}": {
      "snippet": "pptThought{${1:text}}"
    },
    "pptSnippet{}": {
      "snippet": "pptSnippet{${1:file}}"
    },
    "pptSnippet[]{}": {
      "snippet": "pptSnippet[${2:before code}]{${1:file}}"
    },
    "pptQR{}": {
      "snippet": "pptQR{${1:URL}}"
    },
    "pptQR[]{}": {
      "snippet": "pptQR[${2:height}]{${1:URL}}"
    },
    "pptPinQR{}": {
      "snippet": "pptPinQR{${1:URL}}"
    },
    "pptPinQR[]{}": {
      "snippet": "pptPinQR[${2:right|left}]{${1:URL}}"
    }
  },
  "envs": {
    "pptMiddle": {},
    "pptWideOne": {},
    "pptWide{}": {
      "name": "pptWide",
      "snippet": "{${1:numCols}}"
    },
    "enumerate*": {},
    "enumerate*[]": {
      "name": "enumerate*",
      "snippet": "[${1:options}]"
    },
    "itemize*": {},
    "itemize*[]": {
      "name": "itemize*",
      "snippet": "[${1:options}]"
    },
    "description*": {},
    "description*[]": {
      "name": "description*",
      "snippet": "[${1:options}]"
    }
  },
  "options": [
    "template=9x6",
    "scheme=",
    "directory=${1:directory}",
    "nominutes"
  ],
  "keyvals": []
}