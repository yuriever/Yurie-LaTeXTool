  },
  "envs": {},
  "options": [
    "autocolormarks",
    "noautocolormarks"
  ],
  "keyvals": [
    [
      "krukFont=${1:font}",
      "sylSpace=",
      "topMargin=",
      "krukRaise=",
      "sylRuleHeight="
    ],
    [
      "raise="
    ]
  ]
