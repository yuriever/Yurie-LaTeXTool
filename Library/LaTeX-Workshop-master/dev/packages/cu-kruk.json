  "macros": {
    "cuKruk{}{}": {
      "snippet": "cuKruk{${1:neume}}{${2:text}}"
    },
    "cuKruk[]{}{}": {
      "snippet": "cuKruk[${3:options}]{${1:neume}}{${2:text}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "cuKrukPara{}": {
      "snippet": "cuKrukPara{${1:neumes \\\\ ... \\\\ lyrics}}"
    },
