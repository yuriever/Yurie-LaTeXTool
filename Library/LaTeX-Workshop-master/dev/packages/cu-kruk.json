      "snippet": "cuKrukPara[${2:options}]{${1:neumes \\\\ ... \\\\ lyrics}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "cuSup{}{}": {
      "snippet": "cuSup{${1:sup text}}{${2:base text}}"
    },
    "cuSup[]{}{}": {
      "snippet": "cuSup[${3:options}]{${1:sup text}}{${2:base text}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "cuKrukFont": {},
    "cuKrukSylSpace": {},
    "cuKrukTopMargin": {},
    "cuKrukSylRuleHeight": {},
    "cuKrukRaise": {},
