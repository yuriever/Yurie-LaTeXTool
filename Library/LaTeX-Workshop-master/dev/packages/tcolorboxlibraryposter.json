{
  "includes": {
    "tcolorboxlibrarybreakable": [],
    "tcolorboxlibrarymagazine": [],
    "tcolorboxlibraryskins": [],
    "tcolorboxlibraryfitting": []
  },
  "macros": {
    "tcbposterwidth": {},
    "tcbposterheight": {},
    "tcbpostercolspacing": {},
    "tcbposterrowspacing": {},
    "tcbpostercolumns": {},
    "tcbposterrows": {},
    "tcbpostercolwidth": {},
    "tcbposterrowheight": {},
    "tcbposterset{}": {
      "snippet": "tcbposterset{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "posterbox{}{}": {
      "snippet": "posterbox{${1:placement}}{${2:box content}}"
    },
    "posterbox[]{}{}": {
      "snippet": "posterbox[${3:options}]{${1:placement}}{${2:box content}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    }
  },
  "envs": {
    "tcbposter": {},
    "tcbposter[]": {
      "name": "tcbposter",
      "snippet": "[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "posterboxenv{}": {
      "name": "posterboxenv",
      "snippet": "{${1:placement}}"
    },
    "posterboxenv[]{}": {
      "name": "posterboxenv",
      "snippet": "[${2:options}]{${1:placement}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    }
  },
  "options": [],
  "keyvals": [
    [
      "poster={${1:poster options}}",
      "coverage={${1:options}}",
      "no coverage",
      "boxes={${1:options}}",
      "fontsize="
    ],
    [
      "posterset/poster={${1:poster options}}",
      "posterset/coverage={${1:options}}",
      "posterset/no coverage",
      "posterset/boxes={${1:options}}",
      "posterset/fontsize=",
      "poster/columns=${1:number}",
      "poster/rows=${1:number}",
      "poster/colspacing=",
      "poster/rowspacing=",
      "poster/showframe",
      "poster/width=",
      "poster/height=",
      "poster/prefix=${1:name}",
      "posterloc/name=${1:name}",
      "posterloc/column=${1:number}",
      "posterloc/column*=${1:number}",
      "posterloc/span=${1:number}",
      "posterloc/row=${1:number}",
      "posterloc/rowspan=${1:number}",
      "posterloc/fixed height",
      "posterloc/below=${1:name}",
      "posterloc/above=${1:name}",
      "posterloc/at=${1:name}",
      "posterloc/between=${1:name1} and ${2:name2}",
      "posterloc/sequence=${1:sequence}",
      "posterloc/xshift=",
      "posterloc/yshift=",
      "placeholder"
    ]
  ]
}