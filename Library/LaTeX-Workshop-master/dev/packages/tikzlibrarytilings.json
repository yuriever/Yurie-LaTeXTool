{
  "includes": {
    "tikzlibraryspath3": []
  },
  "macros": {
    "DefineTile{}{}{}": {
      "snippet": "DefineTile{${1:name}}{${2:sides}}{${3:vertices}}"
    },
    "BakeTile{}": {
      "snippet": "BakeTile{${1:name}}"
    },
    "TilingDecomposition{}{}{}": {
      "snippet": "TilingDecomposition{${1:type}}{${2:level}}{${3:seed}}"
    },
    "TilingDecomposition[]{}{}{}": {
      "snippet": "TilingDecomposition[${4:TikZ keys}]{${1:type}}{${2:level}}{${3:seed}}"
    },
    "SetTilingPath{}": {
      "snippet": "SetTilingPath{${1:arg}}"
    },
    "TransformAlongSide{}{}": {
      "snippet": "TransformAlongSide{${1:arg1}}{${2:arg2}}"
    },
    "TransformAlongSide*{}{}": {
      "snippet": "TransformAlongSide*{${1:arg1}}{${2:arg2}}"
    },
    "CoordinatesAtVertices{}": {
      "snippet": "CoordinatesAtVertices{${1:arg}}"
    },
    "UseTile{}": {
      "snippet": "UseTile{${1:arg}}"
    },
    "UseTile[]{}": {
      "snippet": "UseTile[${2:opt}]{${1:arg}}"
    }
  },
  "envs": {},
  "options": [],
  "keyvals": []
}