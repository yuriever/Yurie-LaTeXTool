{
  "includes": {
    "tikz": [],
    "tikzlibraryshapes.misc": [],
    "tikzlibrarycalc": [],
    "cnltx-base": []
  },
  "macros": {
    "chordscheme": {},
    "chordscheme[]": {
      "snippet": "chordscheme[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "scales": {},
    "scales[]": {
      "snippet": "scales[${1:options}]",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "setfingering{}{}{}": {
      "snippet": "setfingering{${1:name}}{${2:fingers}}{${3:roots}}"
    },
    "setchordscheme{}": {
      "snippet": "setchordscheme{${1:options}}",
      "keyvalindex": 2,
      "keyvalpos": 0
    },
    "rootsymbol": {},
    "showrootsymbol": {},
    "ringingstring": {},
    "mutedstring": {}
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "fret-number=${1:number}",
      "name=${1:chord symbol}",
      "position=${1:position}",
      "finger=${1:fret}/${2:string}:${3:label}",
      "root=${1:fret}/${2:string}:${3:label}",
      "show-root=${1:fret}/${2:string}",
      "barre=${1:fret}/${2:string range}:${3:label}",
      "ring={${1:strings}}",
      "mute={${1:strings}}"
    ],
    [
      "fret-number=${1:number}",
      "name=${1:title}",
      "position=${1:position}",
      "finger=${1:fret}/${2:string}:${3:label}",
      "root=${1:fret}/${2:string}:${3:label}",
      "fingering=",
      "fingering*=",
      "fingering?="
    ],
    [
      "x-unit=",
      "y-unit=",
      "rotate=${1:angle}",
      "finger-format={${1:TeX code}}",
      "finger-format+={${1:TeX code}}",
      "position-format={${1:TeX code}}",
      "position-format+={${1:TeX code}}",
      "name-format={${1:TeX code}}",
      "name-format+={${1:TeX code}}",
      "name-below",
      "name-distance=",
      "chord-name-cs=${1:cs}",
      "scales-name-cs=${1:cs}",
      "string-name-format={${1:TeX code}}",
      "string-name-format+={${1:TeX code}}",
      "strings=${1:number}",
      "chord-frets=${1:number}",
      "scales-frets=${1:number}",
      "line-width=",
      "finger-radius=${1:number}",
      "finger-x-offset=${1:number}",
      "finger-y-offset=${1:number}",
      "finger-style={${1:TikZ style}}",
      "root-style={${1:TikZ style}}",
      "show-root-style={${1:TikZ style}}",
      "ringing-style={${1:TikZ style}}",
      "muted-style={${1:TikZ style}}",
      "tuning={${1:strings}}",
      "restrict-bounding-box"
    ]
  ]
}