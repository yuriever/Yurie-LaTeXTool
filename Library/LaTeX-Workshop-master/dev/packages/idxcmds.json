    },
    "newsubmainidxcmd{}{}": {
      "snippet": "newsubmainidxcmd{${1:cmd}}{${2:definition}}"
    },
    "newsubidxcmd[]{}{}": {
      "snippet": "newsubidxcmd[${3:index cmd}]{${1:cmd}}{${2:definition}}"
    },
    "newsubidxcmd{}{}[]": {
      "snippet": "newsubidxcmd{${1:cmd}}{${2:main entry}}[${3:append}]"
