      "snippet": "newsubidxcmd{${1:cmd}}{${2:main entry}}{${3:definition}}[${4:append}]"
    },
    "newsubidxcmd[]{}{}{}[]": {
      "snippet": "newsubidxcmd[${4:index cmd}]{${1:cmd}}{${2:main entry}}{${3:definition}}[${5:append}]"
    },
    "newsubidxcmd*{}{}{}": {
      "snippet": "newsubidxcmd*{${1:cmd}}{${2:main entry}}{${3:definition}}"
    },
    "newsubidxcmd*[]{}{}{}": {
      "snippet": "newsubidxcmd*[${4:index cmd}]{${1:cmd}}{${2:main entry}}{${3:definition}}"
