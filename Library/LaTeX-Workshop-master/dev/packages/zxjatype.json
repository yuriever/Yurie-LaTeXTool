{
  "includes": {
    "ifxetex": [],
    "xeCJK": []
  },
  "macros": {
    "bxDebug{}": {
      "snippet": "bxDebug{${1:text}}"
    },
    "zxJaFamilyFontHook": {},
    "zxJaFamilyName": {},
    "inhibitglue": {},
    "setjamainfont{}": {
      "snippet": "setjamainfont{${1:font name}}"
    },
    "setjamainfont{}[]": {
      "snippet": "setjamainfont{${1:font name}}[${2:font features}]"
    },
    "setjamainfont[]{}": {
      "snippet": "setjamainfont[${2:font features}]{${1:font name}}"
    },
    "setjasansfont{}": {
      "snippet": "setjasansfont{${1:font name}}"
    },
    "setjasansfont{}[]": {
      "snippet": "setjasansfont{${1:font name}}[${2:font features}]"
    },
    "setjasansfont[]{}": {
      "snippet": "setjasansfont[${2:font features}]{${1:font name}}"
    },
    "setjamonofont{}": {
      "snippet": "setjamonofont{${1:font name}}"
    },
    "setjamonofont{}[]": {
      "snippet": "setjamonofont{${1:font name}}[${2:font features}]"
    },
    "setjamonofont[]{}": {
      "snippet": "setjamonofont[${2:font features}]{${1:font name}}"
    },
    "setjafamilyfont{}{}": {
      "snippet": "setjafamilyfont{${1:family}}{${2:font name}}"
    },
    "setjafamilyfont{}{}[]": {
      "snippet": "setjafamilyfont{${1:family}}{${2:font name}}[${3:font features}]"
    },
    "setjafamilyfont{}[]{}": {
      "snippet": "setjafamilyfont{${1:family}}[${2:font features}]{${3:font name}}"
    },
    "jafamily{}": {
      "snippet": "jafamily{${1:family}}"
    },
    "zxjapanesestyle": {},
    "zxusejapaneseparameters": {},
    "zxuseoriginalparameters": {},
    "setjafontscale{}": {
      "snippet": "setjafontscale{${1:factor}}"
    },
    "jafamilyinverbatim": {},
    "nojafamilyinverbatim": {},
    ">": {
      "detail": ">",
      "documentation": "GREATER-THAN SIGN r:"
    },
    "<": {
      "detail": "<",
      "documentation": "LESS-THAN SIGN r:"
    },
    "textrawja{}": {
      "snippet": "textrawja{${1:text}}"
    },
    "textrawen{}": {
      "snippet": "textrawen{${1:text}}"
    }
  },
  "envs": {
    "rawjatext": {},
    "rawentext": {}
  },
  "options": [
    "default",
    "nodefault",
    "CJKchecksingle",
    "noCJKchecksingle",
    "LocalConfig=",
    "xeCJKactive",
    "CJKspace",
    "CJKmath",
    "CJKglue=${1:glue}",
    "CJKecglue=${1:glue}",
    "xCJKecglue=${1:glue}",
    "CheckSingle",
    "WindowPenalty=${1:integer}",
    "PlainEquation",
    "NewLineCS={${1:commands}}",
    "NewLineCS+={${1:commands}}",
    "NewLineCS-={${1:commands}}",
    "EnvCS={${1:commands}}",
    "EnvCS+={${1:commands}}",
    "EnvCS-={${1:commands}}",
    "InlineEnv={${1:env1,env2,...}}",
    "InlineEnv+={${1:env1,env2,...}}",
    "InlineEnv-={${1:env1,env2,...}}",
    "AutoFallback",
    "AutoFakeBold=${1:true,false,or <number>}",
    "AutoFakeSlant=${1:true,false,or <number>}",
    "EmboldenFactor=${1:factor}",
    "SlantFactor=${1:factor}",
    "PunctStyle=",
    "PunctFamily=${1:false or <family>}",
    "KaiMingPunct={${1:punctuation}}",
    "KaiMingPunct+={${1:punctuation}}",
    "KaiMingPunct-={${1:punctuation}}",
    "LongPunct={${1:punctuation}}",
    "LongPunct+={${1:punctuation}}",
    "LongPunct-={${1:punctuation}}",
    "MiddlePunct={${1:punctuation}}",
    "MiddlePunct+={${1:punctuation}}",
    "MiddlePunct-={${1:punctuation}}",
    "PunctWidth=",
    "PunctBoundWidth=",
    "AllowBreakBetweenPuncts",
    "RubberPunctSkip=",
    "CheckFullRight",
    "NoBreakCS={${1:commands}}",
    "NoBreakCS+={${1:commands}}",
    "NoBreakCS-={${1:commands}}",
    "Verb=",
    "LoadFandol"
  ],
  "keyvals": []
}