{
  "includes": {
    "eforms": [],
    "graphicx": [],
    "graphicxbox": [],
    "opacity-pro": []
  },
  "macros": {
    "digiCap{}{}": {
      "snippet": "digiCap{${1:imagefile}}{${2:caption}}"
    },
    "digiCap{}[]{}": {
      "snippet": "digiCap{${1:imagefile}}[${2:design keyvals}]{${3:caption}}"
    },
    "digiCap[]{}{}": {
      "snippet": "digiCap[${3:pos keyvals}]{${1:imagefile}}{${2:caption}}"
    },
    "digiCap[]{}[]{}": {
      "snippet": "digiCap[${3:pos keyvals}]{${1:imagefile}}[${4:design keyvals}]{${2:caption}}"
    },
    "digiCap*{}{}": {
      "snippet": "digiCap*{${1:imagefile}}{${2:caption}}"
    },
    "digiCap*{}[]{}": {
      "snippet": "digiCap*{${1:imagefile}}[${2:design keyvals}]{${3:caption}}"
    },
    "digiCap*[]{}{}": {
      "snippet": "digiCap*[${3:pos keyvals}]{${1:imagefile}}{${2:caption}}"
    },
    "digiCap*[]{}[]{}": {
      "snippet": "digiCap*[${3:pos keyvals}]{${1:imagefile}}[${4:design keyvals}]{${2:caption}}"
    },
    "graphicHeight": {},
    "graphicWidth": {},
    "opcolorbox{}": {
      "snippet": "opcolorbox{${1:text}}"
    },
    "opcolorbox[]{}": {
      "snippet": "opcolorbox[${2:options}]{${1:text}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "PicsThisDoc{{}{}{}{}": {
      "snippet": "PicsThisDoc{{${1:name1}}{${2:path1}}{${3:title1}}{${4:caption1}...}}",
      "option": "display1"
    },
    "presentationOrder{}": {
      "snippet": "presentationOrder{${1:name1,name2,...}}",
      "option": "display1"
    },
    "digiDisplaySpace{}{}": {
      "snippet": "digiDisplaySpace{${1:height}}{${2:width}}",
      "option": "display1"
    },
    "insertCaptions": {
      "option": "display1"
    },
    "insertThumbs{}{}": {
      "snippet": "insertThumbs{${1:rows}}{${2:columns}}",
      "option": "display1"
    },
    "dcFirstOpt{}": {
      "snippet": "dcFirstOpt{${1:pos keyvals}}",
      "option": "display1"
    },
    "dcFirstOpt[]{}": {
      "snippet": "dcFirstOpt[${2:name}]{${1:pos keyvals}}",
      "option": "display1"
    },
    "dcSecondOpt{}": {
      "snippet": "dcSecondOpt{${1:design keyvals}}",
      "option": "display1"
    },
    "dcSecondOpt[]{}": {
      "snippet": "dcSecondOpt[${2:name}]{${1:design keyvals}}",
      "option": "display1"
    },
    "useRollovers": {
      "option": "display1"
    },
    "noRollovers": {
      "option": "display1"
    },
    "longCapFmt{}": {
      "snippet": "longCapFmt{${1:format cmds}}",
      "option": "display1"
    },
    "shortCapFmt{}": {
      "snippet": "shortCapFmt{${1:format cmds}}",
      "option": "display1"
    },
    "setThumbAppearances{}": {
      "snippet": "setThumbAppearances{${1:keyvals}}",
      "option": "display1",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "setThumbAppearances[]{}": {
      "snippet": "setThumbAppearances[${2:name}]{${1:keyvals}}",
      "option": "display1",
      "keyvalindex": 1,
      "keyvalpos": 1
    },
    "setWidthOfThumbs{}": {
      "snippet": "setWidthOfThumbs{${1:width}}",
      "option": "display1"
    },
    "addvspacetorows{}": {
      "snippet": "addvspacetorows{${1:length}}",
      "option": "display1"
    },
    "digiDSWidth": {
      "option": "display1"
    },
    "digiDSHeight": {
      "option": "display1"
    },
    "insertPhotos": {
      "option": "display1"
    },
    "normalAppr{}": {
      "snippet": "normalAppr{${1:arg}}",
      "option": "display1"
    },
    "downAppr{}": {
      "snippet": "downAppr{${1:arg}}",
      "option": "display1"
    },
    "rolloverAppr{}": {
      "snippet": "rolloverAppr{${1:arg}}",
      "option": "display1"
    },
    "digiCapsPresets{}": {
      "snippet": "digiCapsPresets{${1:arg}}"
    },
    "hiddenPresets": {}
  },
  "envs": {},
  "options": [
    "display1"
  ],
  "keyvals": [
    [
      "borderwidth=",
      "fboxsep=",
      "width=",
      "bordercolor=",
      "bgcolor=",
      "borderop=${1:factor}",
      "bgop=${1:factor}",
      "textop=${1:factor}",
      "borderblendmode=${1:mode}",
      "bgblendmode=${1:mode}"
    ],
    [
      "normalop=${1:factor}",
      "rolloverop=${1:factor}",
      "downop=${1:factor}",
      "boundarywidth",
      "rgbcolor=${1:r g b}",
      "cmykcolor=${1:c m y k}"
    ]
  ]
}