{
  "includes": {
    "simplekv": []
  },
  "macros": {
    "readgraph{}": {
      "snippet": "readgraph{${1:graph specs}}"
    },
    "readgraph*{}": {
      "snippet": "readgraph*{${1:graph specs}}"
    },
    "dijkstra{}{}": {
      "snippet": "dijkstra{${1:vertex1}}{${2:vertex2}}"
    },
    "dijkstra[]{}{}": {
      "snippet": "dijkstra[${3:options}]{${1:vertex1}}{${2:vertex2}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "setdijk{}": {
      "snippet": "setdijk{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "setdijkdefault{}": {
      "snippet": "setdijkdefault{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "dijkdist": {},
    "dijkpath": {},
    "initdijk": {},
    "formatnodewithprev{}{}": {
      "snippet": "formatnodewithprev{${1:arg1}}{${2:arg2}}"
    },
    "highlightfirstnode{}": {
      "snippet": "highlightfirstnode{${1:arg}}"
    },
    "highlightnode{}{}": {
      "snippet": "highlightnode{${1:arg1}}{${2:arg2}}"
    },
    "dijkname": {},
    "dijkver": {},
    "dijkdate": {}
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "show-tab",
      "v-position=",
      "pre-tab=${1:code}",
      "post-tab=${1:code}",
      "col-type=",
      "infinity-code=${1:code}",
      "norevisit-code=${1:code}",
      "h-rules",
      "show-lastcol",
      "lastcol-type=",
      "lastcol-label=",
      "nopath-string=",
      "path-sep="
    ]
  ]
}