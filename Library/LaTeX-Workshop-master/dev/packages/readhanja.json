  "macros": {
    "readhanjahangulfont{}": {
      "snippet": "readhanjahangulfont{${1:font}}"
    },
    "readhanjahangulfont[]{}": {
      "snippet": "readhanjahangulfont[${2:font features}]{${1:font}}"
    },
    "readhanjahangulfont{}[]": {
      "snippet": "readhanjahangulfont{${1:font}}[${2:font features}]"
    },
    "readhanjahangulfont[]{}[]": {
      "snippet": "readhanjahangulfont[${2:font features}]{${1:font}}[${3:font features}]"
    },
    "readhanjaraise{}": {
      "snippet": "readhanjaraise{${1:dimen}}"
    },
    "readhanjalocate{}": {
      "snippet": "readhanjalocate{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "readhanjaunit{}": {
      "snippet": "readhanjaunit{${1:options}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "readhanjareading{}{}": {
      "snippet": "readhanjareading{${1:hanja}}{${2:reading}}"
    },
    "readhanjadictionary{}{}": {
      "snippet": "readhanjadictionary{${1:hanja}}{${2:reading}}"
    },
    "readhanjatohangul": {}
  },
