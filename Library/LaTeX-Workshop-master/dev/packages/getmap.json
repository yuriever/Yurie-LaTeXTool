{
  "includes": {
    "xkeyval": [],
    "stringenc": [],
    "ifthen": [],
    "shellesc": [],
    "ifxetex": []
  },
  "macros": {
    "getmap{}": {
      "snippet": "getmap{${1:address}}"
    },
    "getmap[]{}": {
      "snippet": "getmap[${2:options}]{${1:address}}"
    }
  },
  "envs": {},
  "options": [
    "mode=",
    "inputencoding=${1:encoding}",
    "overwrite",
    "file=${1:file name}",
    "key=${1:service key}",
    "scale=${1:number}",
    "zoom=${1:number}",
    "xsize=${1:width in pixels}",
    "ysize=${1:height in pixels}",
    "imagetype=",
    "type=",
    "color=${1:color}",
    "number=${1:number}",
    "language=${1:lang code}",
    "markers={${1:&markers=...}}",
    "visible={${1:loc1|loc2|...}}",
    "path={${1:&path=...}}",
    "pathfile=${1:file name}",
    "heading=${1:degrees}",
    "pitch=${1:degrees}",
    "fov=${1:degrees}"
  ],
  "keyvals": []
}