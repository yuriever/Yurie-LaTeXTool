{
  "includes": {
    "xcolor": []
  },
  "macros": {
    "definespotcolor{}{}{}": {
      "snippet": "definespotcolor{${1:name}}{${2:PDF name}}{${3:CMYK-equivalent}}"
    },
    "definespotcolor{}{}[]{}": {
      "snippet": "definespotcolor{${1:name}}{${2:PDF name}}[${3:alt=lab}]{${4:lab-values}}"
    },
    "definecolorspace{}{}{}": {
      "snippet": "definecolorspace{${1:name}}{${2:mixed}}{${3:color-list}}"
    },
    "definecolorspace*{}{}{}": {
      "snippet": "definecolorspace*{${1:name}}{${2:iccbased}}{${3:icc-file}}"
    },
    "pagecolorspace{}": {
      "snippet": "pagecolorspace{${1:colorspace list}}"
    },
    "resetpagecolorspace": {},
    "overprintstate{}": {
      "snippet": "overprintstate{${1:state}}"
    },
    "textoverprint{}": {
      "snippet": "textoverprint{${1:text}}"
    },
    "textoverprint[]{}": {
      "snippet": "textoverprint[${2:state}]{${1:text}}"
    }
  },
  "envs": {},
  "options": [
    "illuminant=",
    "overprint",
    "opm=",
    "knockout"
  ],
  "keyvals": []
}