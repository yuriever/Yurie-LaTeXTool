{
  "includes": {
    "tikz": []
  },
  "macros": {
    "pgfspectra": {},
    "pgfspectra[]": {
      "snippet": "pgfspectra[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "wlcolor{}": {
      "snippet": "wlcolor{${1:wavelength}}"
    },
    "pgfspectraStyle[]": {
      "snippet": "pgfspectraStyle[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "pgfspectraStyleReset": {},
    "usepgfspectralibrary{}": {
      "snippet": "usepgfspectralibrary{${1:libraries}}"
    },
    "pgfspectradata{}": {
      "snippet": "pgfspectradata{${1:options}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "pgfspectradata[]{}": {
      "snippet": "pgfspectradata[${2:name}]{${1:options}}",
      "keyvalindex": 1,
      "keyvalpos": 1
    },
    "pgfspectratable": {},
    "pgfspectratable{}": {
      "snippet": "pgfspectratable{${1:dataset names}}"
    },
    "pgfspectratable[]": {
      "snippet": "pgfspectratable[${1:options}]",
      "keyvalindex": 2,
      "keyvalpos": 0
    },
    "pgfspectratable[]{}": {
      "snippet": "pgfspectratable[${2:options}]{${1:dataset names}}",
      "keyvalindex": 2,
      "keyvalpos": 0
    },
    "pgfspectrawrite": {},
    "pgfspectrawrite{}": {
      "snippet": "pgfspectrawrite{${1:dataset names}}"
    },
    "pgfspectrawrite[]": {
      "snippet": "pgfspectrawrite[${1:filename}]"
    },
    "pgfspectrawrite[]{}": {
      "snippet": "pgfspectrawrite[${2:filename}]{${1:dataset names}}"
    },
    "pgfspectraplotmap{}": {
      "snippet": "pgfspectraplotmap{${1:name}}"
    },
    "pgfspectraplotmap[]{}": {
      "snippet": "pgfspectraplotmap[${2:l|h}]{${1:name}}"
    },
    "pgfspectraplotshade{}": {
      "snippet": "pgfspectraplotshade{${1:name}}"
    },
    "pgfspectraplotshade[]{}": {
      "snippet": "pgfspectraplotshade[${2:options}]{${1:name}}",
      "keyvalindex": 3,
      "keyvalpos": 0
    },
    "tempercolor{}": {
      "snippet": "tempercolor{${1:Kelvin}}"
    },
    "pgfspectrarainbow{}": {
      "snippet": "pgfspectrarainbow{${1:radius}}"
    },
    "wldez": {},
    "wlquatromil": {}
  },
  "envs": {},
  "options": [
    "LSE",
    "NIST"
  ],
  "keyvals": [
    [
      "use visible shading",
      "width=",
      "height=",
      "element=${1:symbol(s)}",
      "charge=${1:<integer> or all}",
      "Imin=${1:factor}",
      "relative intensity",
      "relative intensity threshold=${1:factor}",
      "line intensity=${1:number}",
      "gamma=${1:number}",
      "brightness=${1:factor}",
      "back=",
      "backIRUV=",
      "IRcolor=",
      "UVcolor=",
      "lines={${1:wavelengths}}",
      "line width=",
      "begin=${1:wavelength}",
      "end=${1:wavelength}",
      "absorption",
      "axis",
      "axis step=${1:integer}",
      "axis ticks=${1:integer}",
      "axis unit=",
      "axis unit precision=${1:integer}",
      "axis label",
      "axis label text={${1:text}}",
      "axis label position=",
      "axis color=",
      "axis font=${1:font commands}",
      "axis font color=",
      "label",
      "label position=",
      "label font=${1:font commands}",
      "label font color=",
      "label before text=${1:text}",
      "label after text=${1:text}",
      "redshift=${1:value}",
      "show redshift value"
    ],
    [
      "element=${1:symbol(s)}",
      "charge=${1:<integer> or all}",
      "Imin=${1:factor}",
      "redshift=${1:value}",
      "begin=${1:wavelength}",
      "end=${1:wavelength}",
      "relative intensity",
      "precision=${1:integer}",
      "unit="
    ],
    [
      "title={${1:text}}",
      "back color=",
      "data back color=",
      "text color=",
      "width=",
      "elements column width="
    ],
    [
      "shade begin=${1:value}",
      "shade end=${1:value}",
      "shade opacity=${1:factor}",
      "shade opacity color=",
      "logarithmic"
    ]
  ]
}