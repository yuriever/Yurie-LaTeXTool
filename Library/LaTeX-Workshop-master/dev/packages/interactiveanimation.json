{
  "includes": {
    "ifpdf": [],
    "keyval": []
  },
  "macros": {
    "controlbutton{}{}{}": {
      "snippet": "controlbutton{${1:caption}}{${2:target frame}}{${3:options}}",
      "keyvalindex": 0,
      "keyvalpos": 2
    },
    "controlbutton[]{}{}{}": {
      "snippet": "controlbutton[${4:sequence file,start page,end page}]{${1:caption}}{${2:target frame}}{${3:options}}",
      "keyvalindex": 0,
      "keyvalpos": 3
    }
  },
  "envs": {
    "animation{}{}": {
      "name": "animation",
      "snippet": "{${1:width}}{${2:height}}"
    },
    "aframe{}{}": {
      "name": "aframe",
      "snippet": "{${1:frame name}}{${2:imagefile,filepage}}"
    }
  },
  "options": [],
  "keyvals": [
    [
      "X=",
      "Y=",
      "position=",
      "width=${1:integer}",
      "height=${1:integer}",
      "scale=${1:factor}",
      "hidden",
      "transparent",
      "span=${1:milliseconds}",
      "keep"
    ]
  ]
}