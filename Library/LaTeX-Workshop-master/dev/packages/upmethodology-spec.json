{
  "includes": {
    "upmethodology-p-common": [],
    "ulem": [],
    "upmethodology-fmt": [],
    "upmethodology-code": []
  },
  "macros": {
    "speccons{}{}": {
      "snippet": "speccons{${1:name}}{${2:prototype}}"
    },
    "speccons[]{}{}": {
      "snippet": "speccons[${3:modifiers}]{${1:name}}{${2:prototype}}"
    },
    "specget{}{}": {
      "snippet": "specget{${1:return type}}{${2:name}}"
    },
    "specget[]{}{}": {
      "snippet": "specget[${3:modifiers}]{${1:return type}}{${2:name}}"
    },
    "specset{}{}": {
      "snippet": "specset{${1:name}}{${2:prototype}}"
    },
    "specset[]{}{}": {
      "snippet": "specset[${3:modifiers}]{${1:name}}{${2:prototype}}"
    },
    "specfunc{}{}{}": {
      "snippet": "specfunc{${1:return type}}{${2:name}}{${3:prototype}}"
    },
    "specfunc[]{}{}{}": {
      "snippet": "specfunc[${4:modifiers}]{${1:return type}}{${2:name}}{${3:prototype}}"
    },
    "specreturn{}": {
      "snippet": "specreturn{${1:description}}"
    },
    "specglobalreturn{}": {
      "snippet": "specglobalreturn{${1:description}}"
    },
    "specparam{}{}": {
      "snippet": "specparam{${1:name}}{${2:description}}"
    },
    "specparam[]{}{}": {
      "snippet": "specparam[${3:in|inout|out}]{${1:name}}{${2:description}}"
    },
    "specendhline": {},
    "specstarthline": {}
  },
  "envs": {
    "detailspec{}": {
      "name": "detailspec",
      "snippet": "{${1:title}}"
    },
    "detailspec[]{}": {
      "name": "detailspec",
      "snippet": "[${2:width}]{${1:title}}"
    },
    "detailspec*{}": {
      "name": "detailspec*",
      "snippet": "{${1:title}}"
    },
    "detailspec*[]{}": {
      "name": "detailspec*",
      "snippet": "[${2:width}]{${1:title}}"
    }
  },
  "options": [
    "french",
    "francais",
    "english"
  ],
  "keyvals": []
}