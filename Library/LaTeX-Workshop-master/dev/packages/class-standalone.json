{
  "includes": {
    "ifluatex": [],
    "ifpdf": [],
    "ifxetex": [],
    "shellesc": [],
    "xkeyval": [],
    "class-beamer": [
      "beamer",
      "beamer=true"
    ],
    "multido": [
      "multido",
      "multido=true"
    ],
    "preview": [
      "preview",
      "preview=true"
    ],
    "pstricks": [
      "pstricks",
      "pstricks=true"
    ],
    "tikz": [
      "tikz",
      "tikz=true"
    ],
    "varwidth": [
      "varwidth",
      "varwidth=true"
    ]
  },
  "macros": {
    "multimathsep": {
      "option": "math=true"
    },
    "multidisplaymathsep": {
      "option": "math=true"
    },
    "standaloneconfig{}": {
      "snippet": "standaloneconfig{${1:options}}"
    },
    "standaloneenv{}": {
      "snippet": "standaloneenv{${1:envname1,envname2,...}}"
    },
    "standaloneignore": {},
    "thesapage": {},
    "ifstandalone": {},
    "standalonetrue": {},
    "standalonefalse": {},
    "ifstandalonebeamer": {},
    "standalonebeamertrue": {},
    "standalonebeamerfalse": {},
    "IfStandalone{}{}": {
      "snippet": "IfStandalone{${1:code for standalone}}{${2:code for main document}}"
    },
    "onlyifstandalone{}": {
      "snippet": "onlyifstandalone{${1:code}}"
    }
  },
  "envs": {
    "standaloneframe": {
      "option": "beamer=true"
    },
    "standaloneframe<>": {
      "name": "standaloneframe",
      "snippet": "<${1:overlay spec}>",
      "option": "beamer=true"
    },
    "standaloneframe<>[]": {
      "name": "standaloneframe",
      "snippet": "<${2:overlay spec}>[${1:<default overlay spec>}]",
      "option": "beamer=true"
    },
    "standaloneframe<>[][]": {
      "name": "standaloneframe",
      "snippet": "<${3:overlay spec}>[${1:<default overlay spec>}][${2:options}]",
      "option": "beamer=true"
    },
    "standaloneframe<>[][]{}": {
      "name": "standaloneframe",
      "snippet": "<${4:overlay spec}>[${2:<default overlay spec>}][${3:options}]{${1:title}}",
      "option": "beamer=true"
    },
    "standaloneframe<>[][]{}{}": {
      "name": "standaloneframe",
      "snippet": "<${5:overlay spec}>[${3:<default overlay spec>}][${4:options}]{${1:title}}{${2:subtitle}}",
      "option": "beamer=true"
    },
    "standaloneframe[]": {
      "name": "standaloneframe",
      "snippet": "[${1:options}]",
      "option": "beamer=true"
    },
    "standaloneframe[][]": {
      "name": "standaloneframe",
      "snippet": "[${1:<default overlay spec>}][${2:options}]",
      "option": "beamer=true"
    },
    "standaloneframe[][]{}": {
      "name": "standaloneframe",
      "snippet": "[${2:<default overlay spec>}][${3:options}]{${1:title}}",
      "option": "beamer=true"
    },
    "standaloneframe[][]{}{}": {
      "name": "standaloneframe",
      "snippet": "[${3:<default overlay spec>}][${4:options}]{${1:title}}{${2:subtitle}}",
      "option": "beamer=true"
    },
    "standaloneframe[]{}": {
      "name": "standaloneframe",
      "snippet": "[${2:options}]{${1:title}}",
      "option": "beamer=true"
    },
    "standaloneframe[]{}{}": {
      "name": "standaloneframe",
      "snippet": "[${3:options}]{${1:title}}{${2:subtitle}}",
      "option": "beamer=true"
    },
    "standaloneframe{}": {
      "name": "standaloneframe",
      "snippet": "{${1:title}}",
      "option": "beamer=true"
    },
    "multimath": {
      "option": "math=true"
    },
    "multidisplaymath": {
      "option": "math=true"
    },
    "standalone": {}
  },
  "options": [
    "beamer",
    "border=",
    "border={${1:<l/r> <t/b>}}",
    "border={${1:<l> <b> <r> <t>}}",
    "class=${1:class name}",
    "convert",
    "convert={${1:conversion options}}",
    "crop",
    "float",
    "float",
    "gif",
    "gif={${1:conversion options}}",
    "ignoreempty",
    "ignorerest",
    "jpg",
    "jpg={${1:conversion options}}",
    "margin=",
    "math",
    "multi",
    "multi={${1:envname1,envname2,...}}",
    "multido",
    "png",
    "png={${1:conversion options}}",
    "preview",
    "pstricks",
    "tikz",
    "varwidth",
    "varwidth=",
    "10pt",
    "11pt",
    "12pt"
  ],
  "keyvals": []
}