{
  "includes": {
    "trimspaces": [],
    "xkeyval": []
  },
  "macros": {
    "multidef{}{}": {
      "snippet": "multidef{${1:definition}}{${2:csname list}}"
    },
    "multidef[]{}{}": {
      "snippet": "multidef[${3:options}]{${1:definition}}{${2:csname list}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "arg=${1:num of args}",
      "noerr",
      "nowarn",
      "prefix=${1:prefix}",
      "p=${1:prefix}",
      "suffix=${1:suffix}",
      "s=${1:suffix}",
      "long",
      "global",
      "robust"
    ]
  ]
}