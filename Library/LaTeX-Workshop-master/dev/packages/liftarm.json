    "tikzlibrarycalc": []
  },
  "macros": {
    "liftarm{}{}{}": {
      "snippet": "liftarm{${1:point}}{${2:length}}{${3:angle}}"
    },
    "liftarm[]{}{}{}": {
      "snippet": "liftarm[${4:options}]{${1:point}}{${2:length}}{${3:angle}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "liftarmconnect{}{}{}{}": {
      "snippet": "liftarmconnect{${1:point1}}{${2:length1}}{${3:point2}}{${4:length2}}"
    },
    "liftarmconnect[]{}{}{}{}": {
      "snippet": "liftarmconnect[${5:options}]{${1:point1}}{${2:length1}}{${3:point2}}{${4:length2}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "liftarmconstruct{}{}": {
      "snippet": "liftarmconstruct{${1:text}}{${2:commands}}"
    },
    "liftarmconstruct[]{}{}": {
      "snippet": "liftarmconstruct[${3:TikZ options}]{${1:text}}{${2:commands}}"
    },
    "liftarmanimate{}{}{}": {
      "snippet": "liftarmanimate{${1:frame rate}}{${2:list}}{${3:commands}}"
    },
    "liftarmanimate[]{}{}{}": {
      "snippet": "liftarmanimate[${4:options}]{${1:frame rate}}{${2:list}}{${3:commands}}",
      "keyvalindex": 2,
      "keyvalpos": 0
    }
  },
  "envs": {
    "liftarmconstruction": {},
    "liftarmconstruction[]": {
      "name": "liftarmconstruction",
      "snippet": "[${1:TikZ options}]"
    }
  },
