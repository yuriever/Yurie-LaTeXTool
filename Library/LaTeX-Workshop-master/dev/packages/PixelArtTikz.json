{
  "includes": {
    "tikz": [],
    "simplekv": [],
    "xintexpr": [],
    "xinttools": [],
    "xstring": [],
    "listofitems": [],
    "csvsimple-l3": []
  },
  "macros": {
    "PixelArtTikz{}": {
      "snippet": "PixelArtTikz{${1:file}}"
    },
    "PixelArtTikz[]{}": {
      "snippet": "PixelArtTikz[${2:clés}]{${1:file}}"
    },
    "PixelArtTikz[]<>{}": {
      "snippet": "PixelArtTikz[${2:clés}]<${3:TikZ options}>{${1:file}}"
    },
    "PixelArtTikz*{}": {
      "snippet": "PixelArtTikz*{${1:file}}"
    },
    "PixelArtTikz*[]{}": {
      "snippet": "PixelArtTikz*[${2:clés}]{${1:file}}"
    },
    "MiniPixelArt{}": {
      "snippet": "MiniPixelArt{${1:liste des couleurs}}"
    },
    "MiniPixelArt{}<>": {
      "snippet": "MiniPixelArt{${1:liste des couleurs}}<${2:TikZ options}>"
    },
    "MiniPixelArt[]{}": {
      "snippet": "MiniPixelArt[${2:clés}]{${1:liste des couleurs}}"
    },
    "MiniPixelArt[]{}<>": {
      "snippet": "MiniPixelArt[${2:clés}]{${1:liste des couleurs}}<${3:TikZ options}>"
    },
    "PixlArtTikz{}": {
      "snippet": "PixlArtTikz{${1:file}}"
    },
    "PixlArtTikz[]{}": {
      "snippet": "PixlArtTikz[${2:options}]{${1:file}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "PixlArtTikz[]<>{}": {
      "snippet": "PixlArtTikz[${2:options}]<${3:TikZ options}>{${1:file}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "PixlArtTikz*{}": {
      "snippet": "PixlArtTikz*{${1:file}}"
    },
    "PixlArtTikz*[]{}": {
      "snippet": "PixlArtTikz*[${2:options}]{${1:file}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "MiniPixlArt{}": {
      "snippet": "MiniPixlArt{${1:list of colors}}"
    },
    "MiniPixlArt{}<>": {
      "snippet": "MiniPixlArt{${1:list of colors}}<${2:TikZ options}>"
    },
    "MiniPixlArt[]{}": {
      "snippet": "MiniPixlArt[${2:options}]{${1:list of colors}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "MiniPixlArt[]{}<>": {
      "snippet": "MiniPixlArt[${2:options}]{${1:list of colors}}<${3:TikZ options}>",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "DessineCarrePA{}{}{}": {
      "snippet": "DessineCarrePA{${1:arg1}}{${2:arg2}}{${3:arg3}}"
    },
    "DrawSquarePA{}{}{}": {
      "snippet": "DrawSquarePA{${1:arg1}}{${2:arg2}}{${3:arg3}}"
    },
    "minipaunit": {},
    "PATchiffres": {},
    "PATcouleurs": {},
    "PATlettres": {},
    "PATtaille": {},
    "PATunit": {},
    "pixchf": {},
    "pixcnt": {},
    "pixcol": {},
    "pixpos": {}
  },
  "envs": {
    "EnvPixelArtTikz{}": {
      "name": "EnvPixelArtTikz",
      "snippet": "{${1:file}}"
    },
    "EnvPixelArtTikz[]{}": {
      "name": "EnvPixelArtTikz",
      "snippet": "[${2:clés}]{${1:file}}"
    },
    "EnvPixelArtTikz[]<>{}": {
      "name": "EnvPixelArtTikz",
      "snippet": "[${2:clés}]<${3:TikZ options}>{${1:file}}"
    },
    "EnvPixlArtTikz{}": {
      "name": "EnvPixlArtTikz",
      "snippet": "{${1:file}}"
    },
    "EnvPixlArtTikz[]{}": {
      "name": "EnvPixlArtTikz",
      "snippet": "[${2:options}]{${1:file}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "EnvPixlArtTikz[]<>{}": {
      "name": "EnvPixlArtTikz",
      "snippet": "[${2:options}]<${3:TikZ options}>{${1:file}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
  "options": [
    "csvii"
  ],
  "keyvals": [
    [
      "Codes=${1:string}",
      "Colors={${1:color1,color2,...}}",
      "Symbols={${1:symbol1,symbol2,...}}",
      "Correction",
      "Symb",
      "Border",
      "Style=${1:font commands}",
      "Unit=${1:number}"
    ],
    [
      "Unit=",
      "Border"
    ]
  ]
}