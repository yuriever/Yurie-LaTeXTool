{
  "includes": {
    "xkeyval": []
  },
  "macros": {
    "newref{}": {
      "snippet": "newref{${1:ref types}}"
    },
    "newref[]{}": {
      "snippet": "newref[${2:options}]{${1:ref types}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "setrefstyle{}{}": {
      "snippet": "setrefstyle{${1:ref types}}{${2:options}}",
      "keyvalindex": 1,
      "keyvalpos": 1
    }
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "namespace=${1:string}",
      "nonamespace",
      "scope=${1:counter}"
    ],
    [
      "refcmd=${1:command}",
      "sep=${1:command}",
      "last sep",
      "last sep=${1:command}",
      "prefix=${1:command}",
      "suffix=${1:command}"
    ]
  ]
}