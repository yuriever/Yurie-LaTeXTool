{
  "includes": {
    "iftex": [],
    "luacode": [],
    "tikz": [],
    "tikzlibrarypatterns": []
  },
  "macros": {
    "pixelart{}": {
      "snippet": "pixelart{${1:pixels}}"
    },
    "pixelart[]{}": {
      "snippet": "pixelart[${2:options}]{${1:pixels}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "tikzpixelart{}{}": {
      "snippet": "tikzpixelart{${1:coord}}{${2:pixels}}"
    },
    "tikzpixelart[]{}{}": {
      "snippet": "tikzpixelart[${3:options}]{${1:coord}}{${2:pixels}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "setpixelartdefault{}": {
      "snippet": "setpixelartdefault{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "pixelartlogo": {},
    "pixelartlogo[]": {
      "snippet": "pixelartlogo[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "pixelartheart": {},
    "pixelartheart[]": {
      "snippet": "pixelartheart[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "pixelartname": {},
    "pixelartname[]": {
      "snippet": "pixelartname[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "pixelartlogobw": {},
    "pixelartlogobw[]": {
      "snippet": "pixelartlogobw[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "pixelartheartbw": {},
    "pixelartheartbw[]": {
      "snippet": "pixelartheartbw[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "pixelartnamebw": {},
    "pixelartnamebw[]": {
      "snippet": "pixelartnamebw[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "newpixelartcolors{}{}": {
      "snippet": "newpixelartcolors{${1:name}}{${2:keyvals}}"
    },
    "renewpixelartcolors{}{}": {
      "snippet": "renewpixelartcolors{${1:name}}{${2:keyvals}}"
    },
    "setpixelartdebugon": {},
    "setpixelartdebugoff": {}
  },
  "envs": {},
  "options": [
    "draft"
  ],
  "keyvals": [
    [
      "colors=${1:options}",
      "draft",
      "margin=",
      "squares",
      "squares={${1:options}}",
      "stack",
      "style=${1:keyword}",
      "tikz={${1:TikZ keys}}"
    ]
  ]
}