{
  "includes": {},
  "macros": {
    "postnote{}": {
      "snippet": "postnote{${1:text}}"
    },
    "postnote[]{}": {
      "snippet": "postnote[${2:options}]{${1:text}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "postnotesection{}": {
      "snippet": "postnotesection{${1:text}}"
    },
    "postnotesection[]{}": {
      "snippet": "postnotesection[${2:options}]{${1:text}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "printpostnotes": {},
    "postnoteref{}": {
      "snippet": "postnoteref{${1:label}}"
    },
    "postnoteref*{}": {
      "snippet": "postnoteref*{${1:label}}"
    },
    "postnotezref{}": {
      "snippet": "postnotezref{${1:label}}"
    },
    "postnotezref*{}": {
      "snippet": "postnotezref*{${1:label}}"
    },
    "postnotesetup{}": {
      "snippet": "postnotesetup{${1:options}}",
      "keyvalindex": 2,
      "keyvalpos": 0
    },
    "pnhdchapfirst": {},
    "pnhdchaplast": {},
    "pnhdnamefirst": {},
    "pnhdnamelast": {},
    "pnhdnotes": {},
    "pnhdpagefirst": {},
    "pnhdpagelast": {},
    "pnhdsectfirst": {},
    "pnhdsectlast": {},
    "pnhdtopage": {},
    "pnhdtopages": {},
    "pnheaderdefault": {},
    "pnheading": {},
    "pnidnextnote": {},
    "pnthechapter": {},
    "pnthechapternextnote": {},
    "pnthepage": {},
    "pnthesection": {},
    "pnthesectionnextnote": {},
    "pntitle": {},
    "thepostnote": {},
    "thepostnotesection": {},
    "thepostnotetext": {},
    "postnotesectionx{}": {
      "snippet": "postnotesectionx{${1:text}}"
    },
    "postnotesectionx[]{}": {
      "snippet": "postnotesectionx[${2:options}]{${1:text}}"
    }
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "mark=${1:number}",
      "markstr=${1:string}",
      "sortnum=${1:number}",
      "nomark",
      "label=",
      "zlabel="
    ],
    [
      "name=${1:name}",
      "exp"
    ],
    [
      "heading={${1:text}}",
      "format={${1:font commands}}",
      "listenv=${1:envname}",
      "makemark={${1:code}}",
      "maketextmark={${1:code}}",
      "pretextmark={${1:code}}",
      "posttextmark={${1:code}}",
      "postprintnote={${1:code}}",
      "style=",
      "hyperref=",
      "backlink",
      "sort"
    ]
  ]
}