{
  "includes": {
    "xkeyval": [],
    "geometry": [],
    "ifxetex": [],
    "hyperref": [],
    "graphicx": [],
    "pstricks": [],
    "pst-ovl": [],
    "xcolor": [],
    "enumitem": [],
    "verbatim": [],
    "powerdot-default": [],
    "pdfbase": [
      "clock",
      "clock=true"
    ],
    "powerdot-aggie": [
      "style=aggie"
    ],
    "powerdot-BerlinFU": [
      "style=BerlinFU"
    ],
    "powerdot-bframe": [
      "style=bframe"
    ],
    "powerdot-ciment": [
      "style=ciment"
    ],
    "powerdot-elcolors": [
      "style=elcolors"
    ],
    "powerdot-fyma": [
      "style=fyma"
    ],
    "powerdot-horatio": [
      "style=horatio"
    ],
    "powerdot-husky": [
      "style=husky"
    ],
    "powerdot-ikeda": [
      "style=ikeda"
    ],
    "powerdot-jefka": [
      "style=jefka"
    ],
    "powerdot-klope": [
      "style=klope"
    ],
    "powerdot-paintings": [
      "style=paintings"
    ],
    "powerdot-pazik": [
      "style=pazik"
    ],
    "powerdot-sailor": [
      "style=sailor"
    ],
    "powerdot-simple": [
      "style=simple"
    ],
    "powerdot-tuliplab": [
      "style=tuliplab"
    ],
    "powerdot-tycja": [
      "style=tycja"
    ],
    "powerdot-upen": [
      "style=upen"
    ]
  },
  "macros": {
    "pdsetup{}": {
      "snippet": "pdsetup{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "maketitle[]": {
      "snippet": "maketitle[${1:options}]"
    },
    "pause": {},
    "pause[]": {
      "snippet": "pause[${1:number}]"
    },
    "item<>": {
      "snippet": "item<${1:overlays}>"
    },
    "item[]<>": {
      "snippet": "item[${1:label}]<${2:overlays}>"
    },
    "onslide{}{}": {
      "snippet": "onslide{${1:overlays}}{${2:text}}"
    },
    "onslide*{}{}": {
      "snippet": "onslide*{${1:overlays}}{${2:text}}"
    },
    "tableofcontents[]": {
      "snippet": "tableofcontents[${1:options}]",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "slidewidth": {},
    "slideheight": {},
    "thenote": {},
    "theslide": {},
    "twocolumn{}{}": {
      "snippet": "twocolumn{${1:left}}{${2:right}}"
    },
    "twocolumn[]{}{}": {
      "snippet": "twocolumn[${3:options}]{${1:left}}{${2:right}}",
      "keyvalindex": 2,
      "keyvalpos": 0
    },
    "pddefinetemplate{}{}{}": {
      "snippet": "pddefinetemplate{${1:envname}}{${2:options}}{${3:commands}}"
    },
    "pddefinetemplate[]{}{}{}": {
      "snippet": "pddefinetemplate[${4:basis}]{${1:envname}}{${2:options}}{${3:commands}}"
    },
    "pdifsetup{}{}{}": {
      "snippet": "pdifsetup{${1:desired}}{${2:true}}{${3:false}}"
    },
    "pddefinelyxtemplate{}": {
      "snippet": "pddefinelyxtemplate{${1:cmd}}"
    },
    "lyxend": {},
    "lyxnote": {},
    "lyxslide": {},
    "lyxwideslide": {},
    "lyxemptyslide": {},
    "pdbookmark{}{}{}{}": {
      "snippet": "pdbookmark{${1:arg1}}{${2:arg2}}{${3:arg3}}{${4:arg4}}"
    },
    "pdcontentsline{}{}{}": {
      "snippet": "pdcontentsline{${1:arg1}}{${2:arg2}}{${3:arg3}}"
    }
  },
  "envs": {
    "enumerate[][]": {
      "name": "enumerate",
      "snippet": "[${1:type=0|1,}][${2:enumitem options}]"
    },
    "itemize[][]": {
      "name": "itemize",
      "snippet": "[${1:type=0|1}][${2:enumitem options}]"
    },
    "note{}": {
      "name": "note",
      "snippet": "{${1:title}}"
    },
    "emptyslide{}": {
      "name": "emptyslide",
      "snippet": "{${1:title}}"
    },
    "pauseslide{}": {
      "name": "pauseslide",
      "snippet": "{${1:title}}"
    }
  },
  "options": [
    "mode=",
    "nohandoutpagebreaks",
    "nohandoutframes",
    "paper=",
    "orient=",
    "display=",
    "size=",
    "style=${1:style name}",
    "fleqn",
    "leqno",
    "nopsheader",
    "hlentries",
    "hlsections",
    "pauseslide",
    "clock"
  ],
  "keyvals": [
    [
      "theslide=${1:code}",
      "thenote=${1:code}",
      "counters={${1:counter1,counter2,...}}",
      "list={${1:enumitem options}}",
      "enumerate={${1:enumitem options}}",
      "itemize={${1:enumitem options}}",
      "clockformat=${1:format}",
      "clockrefresh=${1:milliseconds}",
      "palette=${1:palette name}",
      "lf=${1:text}",
      "cf=${1:text}",
      "rf=${1:text}",
      "trans=${1:transition}",
      "method=",
      "logohook=",
      "logopos={${1:width,height}}",
      "logocmd={${1:code}}",
      "randomdots",
      "dmindots=${1:integer}",
      "dmaxdots=${1:integer}",
      "dminsize=",
      "dmaxsize=",
      "dminwidth=",
      "dmaxwidth=",
      "dminheight=",
      "dmaxheight=",
      "dbright=${1:integer}",
      "dprop={${1:pstricks options}}"
    ],
    [
      "type=",
      "content="
    ],
    [
      "lineheight=",
      "lineprop={${1:pstricks options}}",
      "lfrheight=",
      "lfrprop={${1:pstricks options}}",
      "rfrheight=",
      "rfrprop={${1:pstricks options}}",
      "lcolwidth=",
      "rcolwidth=",
      "frsep=",
      "colsep=",
      "topsep=",
      "bottomsep=",
      "indent="
    ]
  ]
}