{
  "includes": {
    "xetex": [],
    "fontspec": [],
    "xunicode": [],
    "kvoptions": [],
    "etoolbox": []
  },
  "macros": {
    "setsampletext{}": {
      "snippet": "setsampletext{${1:text}}"
    },
    "printfont{}{}": {
      "snippet": "printfont{${1:font}}{${2:license}}"
    },
    "printfont[]{}{}": {
      "snippet": "printfont[${3:options}]{${1:font}}{${2:license}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "sampletext{}": {
      "snippet": "sampletext{${1:arg}}"
    },
    "sampletext[]{}": {
      "snippet": "sampletext[${2:opt}]{${1:arg}}"
    },
    "samplefeature{}": {
      "snippet": "samplefeature{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
  "envs": {},
  "options": [
    "size={${1:size1,size2,...}}",
    "features={${1:feat1,feat2,...}}",
    "featuresize=${1:fontsize csname}"
  ],
  "keyvals": [
    [
      "Alternate=${1:feature}",
      "Annotation=",
      "AutoFakeBold=${1:number}",
      "AutoFakeSlant=${1:number}",
      "BoldFeatures={${1:features}}",
      "BoldFont=${1:font name}",
      "BoldItalicFeatures={${1:features}}",
      "BoldItalicFont=${1:font name}",
      "BoldSlantedFeatures={${1:features}}",
      "BoldSlantedFont=${1:font name}",
      "BoldSwashFeatures={${1:features}}",
      "BoldSwashFont=${1:font name}",
      "CJKShape=",
      "CharacterVariant={${1:glyph num list}:${2:variety num}}",
      "CharacterWidth=",
      "Color=",
      "Colour=",
      "Contextuals=",
      "Diacritics=",
      "Extension=${1:extension}",
      "FakeBold=${1:number}",
      "FakeSlant=${1:number}",
      "FakeStretch=${1:number}",
      "Font=${1:font name}",
      "FontFace={${1:series}}{${2:shape}}{${3:font name}}",
      "Fractions=",
      "HyphenChar=${1:character}",
      "IgnoreFontspecFile",
      "ItalicFeatures={${1:features}}",
      "ItalicFont=${1:font name}",
      "Kerning=",
      "Language=${1:language}",
      "LetterSpace=${1:number}",
      "Letters=",
      "Ligatures=",
      "LocalForms=",
      "NFSSFamily=${1:font family}",
      "Numbers=",
      "OpticalSize=${1:size}",
      "Ornament=",
      "Path=${1:file path}",
      "PunctuationSpace=${1:number}",
      "RawFeature=${1:feature tag}",
      "Scale=${1:number}",
      "ScaleAgain=${1:number}",
      "Script=${1:name}",
      "Size=${1:number}",
      "SizeFeatures={${1:features}}",
      "SlantedFeatures={${1:features}}",
      "SlantedFont=${1:font name}",
      "SmallCapsFeatures={${1:features}}",
      "SmallCapsFont=${1:font name}",
      "Style=",
      "StylisticAlternates=${1:feature}",
      "StylisticSet=${1:set number}",
      "SwashFeatures={${1:features}}",
      "SwashFont=${1:font name}",
      "UprightFeatures={${1:features}}",
      "UprightFont=${1:font name}",
      "Variant=${1:integer}",
      "Vertical=",
      "VerticalPosition=",
      "WordSpace=${1:number}",
      "Mapping=${1:xetex-only feature}",
      "Renderer=${1:renderer}"
    ]
  ]
}