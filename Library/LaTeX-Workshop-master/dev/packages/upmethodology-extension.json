{
  "includes": {
    "upmethodology-p-common": []
  },
  "macros": {
    "Ifdefined{}{}": {
      "snippet": "Ifdefined{${1:arg}}{${2:code}}"
    },
    "Ifelsedefined{}{}{}": {
      "snippet": "Ifelsedefined{${1:arg}}{${2:true code}}{${3:false code}}"
    },
    "Ifundefined{}{}": {
      "snippet": "Ifundefined{${1:arg}}{${2:code}}"
    },
    "Ifelseundefined{}{}{}": {
      "snippet": "Ifelseundefined{${1:arg}}{${2:true code}}{${3:false code}}"
    },
    "Get{}": {
      "snippet": "Get{${1:variable}}"
    },
    "GetLang{}{}": {
      "snippet": "GetLang{${1:variable}}{${2:language}}"
    },
    "Set{}{}": {
      "snippet": "Set{${1:variable}}{${2:name}}"
    },
    "Set[]{}{}": {
      "snippet": "Set[${3:language}]{${1:variable}}{${2:name}}"
    },
    "Append{}{}": {
      "snippet": "Append{${1:variable}}{${2:text}}"
    },
    "Append[]{}{}": {
      "snippet": "Append[${3:language}]{${1:variable}}{${2:text}}"
    },
    "Unset{}": {
      "snippet": "Unset{${1:variable}}"
    },
    "Unset[]{}": {
      "snippet": "Unset[${2:language}]{${1:variable}}"
    },
    "DeclareCopyright{}{}{}{}": {
      "snippet": "DeclareCopyright{${1:extname}}{${2:year}}{${3:copyrighter}}{${4:text}}"
    },
    "DeclareCopyright[]{}{}{}{}": {
      "snippet": "DeclareCopyright[${5:language}]{${1:extname}}{${2:year}}{${3:copyrighter}}{${4:text}}"
    },
    "UseExtension{}": {
      "snippet": "UseExtension{${1:extname}}"
    }
  },
  "envs": {},
  "options": [],
  "keyvals": []
}