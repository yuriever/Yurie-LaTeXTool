{
  "includes": {
    "CJKutf8": []
  },
  "macros": {
    "xpinyin{}{}": {
      "snippet": "xpinyin{${1:character}}{${2:pinyin}}"
    },
    "xpinyin[]{}{}": {
      "snippet": "xpinyin[${3:options}]{${1:character}}{${2:pinyin}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "xpinyin*{}": {
      "snippet": "xpinyin*{${1:text}}"
    },
    "xpinyin*[]{}": {
      "snippet": "xpinyin*[${2:options}]{${1:text}}"
    },
    "pinyin{}": {
      "snippet": "pinyin{${1:pinyin}}"
    },
    "pinyin[]{}": {
      "snippet": "pinyin[${2:options}]{${1:pinyin}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "setpinyin{}{}": {
      "snippet": "setpinyin{${1:characters}}{${2:pinyin}}"
    },
    "xpinyinsetup{}": {
      "snippet": "xpinyinsetup{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "disablepinyin": {},
    "enablepinyin": {}
  },
  "envs": {
    "pinyinscope": {},
    "pinyinscope[]": {
      "name": "pinyinscope",
      "snippet": "[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
  "options": [],
  "keyvals": [
    [
      "ratio=${1:number}",
      "vsep=",
      "hsep=",
      "pysep=${1:glue}",
      "font=${1:font}",
      "format=${1:format}",
      "multiple=${1:format}",
      "footnote"
    ]
  ]
}