{
  "includes": {
    "luatex": [],
    "kvoptions": []
  },
  "macros": {
    "py{}": {
      "snippet": "py{${1:code}}"
    },
    "pyq{}": {
      "snippet": "pyq{${1:code}}"
    },
    "pyc{}": {
      "snippet": "pyc{${1:code}}"
    },
    "pycq{}": {
      "snippet": "pycq{${1:code}}"
    },
    "pyfile{}": {
      "snippet": "pyfile{${1:file}}"
    },
    "pyfileq{}": {
      "snippet": "pyfileq{${1:file}}"
    },
    "pyfilerepl{}": {
      "snippet": "pyfilerepl{${1:file}}"
    },
    "pysession{}": {
      "snippet": "pysession{${1:session}}"
    },
    "pyoption{}{}": {
      "snippet": "pyoption{${1:option}}{${2:value}}"
    },
    "pyif{}{}{}": {
      "snippet": "pyif{${1:test}}{${2:then clause}}{${3:else clause}}"
    },
    "PyLTVerbatimEnv": {}
  },
  "envs": {
    "python": {},
    "pythonq": {},
    "pythonrepl": {}
  },
  "options": [
    "executable=${1:path}",
    "ignoreerrors",
    "localimports",
    "shutdown=",
    "verbose"
  ],
  "keyvals": []
}