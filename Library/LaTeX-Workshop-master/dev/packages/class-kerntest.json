  "options": [
    "family=${1:font family}",
    "encoding=${1:font encoding}",
    "series=${1:font series}",
    "shape=${1:font shape}",
    "size=",
    "baselineskip=",
    "designsize=",
    "example=${1:text}",
    "papersize=${1:geometry papersize}",
    "extraname=${1:fontname extension}",
    "color",
    "copyquotation",
    "writeall",
    "footer"
  ],
  "keyvals": [
    [
      "ligaturing=${1:number}",
      "italicizing"
    ]
