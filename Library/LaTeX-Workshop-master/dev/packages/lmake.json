{
  "includes": {},
  "macros": {
    "lcmd{}{}{}": {
      "snippet": "lcmd{${1:command}}{${2:prefix}}{${3:list}}"
    },
    "lmake[]": {
      "snippet": "lmake[${1:keyvals}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "p=${1:pattern}",
      "c=${1:separator}",
      "n=%{last index%>",
      "1=${1:first index}",
      "2=${1:second index}",
      "d=${1:dots}",
      "l={${1:sym1,sym2,...}}"
    ]
  ]
}