    },
    "annotation[]{}{}": {
      "snippet": "annotation[${3:options}]{${1:id}}{${2:text}}"
    },
    "argstyle{}": {
      "snippet": "argstyle{${1:text}}"
    }
  },
  "envs": {
    "af": {},
    "af[]": {
      "name": "af",
      "snippet": "[${1:options}]"
