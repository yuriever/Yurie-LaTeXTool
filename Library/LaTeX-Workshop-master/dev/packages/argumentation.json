      "snippet": "setargumentstyle{${1:style}}"
    },
    "setattackstyle{}": {
      "snippet": "setattackstyle{${1:style}}"
    },
    "setsupportstyle{}": {
      "snippet": "setsupportstyle{${1:style}}"
    },
    "argument{}{}": {
      "snippet": "argument{${1:id}}{${2:name}}"
    },
    "argument[]{}{}": {
      "snippet": "argument[${3:options}]{${1:id}}{${2:name}}"
    },
    "attack{}{}": {
      "snippet": "attack{${1:id1}}{${2:id2}}"
    },
    "attack[]{}{}": {
      "snippet": "attack[${3:options}]{${1:id1}}{${2:id2}}"
    },
    "dualattack{}{}": {
      "snippet": "dualattack{${1:id1}}{${2:id2}}"
    },
    "dualattack[]{}{}": {
      "snippet": "dualattack[${3:options}]{${1:id1}}{${2:id2}}"
    },
    "selfattack{}": {
      "snippet": "selfattack{${1:id}}"
    },
    "selfattack[]{}": {
      "snippet": "selfattack[${2:options}]{${1:id}}"
    },
    "support{}{}": {
      "snippet": "support{${1:id1}}{${2:id2}}"
    },
    "support[]{}{}": {
