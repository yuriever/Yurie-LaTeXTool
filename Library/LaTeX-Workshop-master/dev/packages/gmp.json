{
  "includes": {
    "xkeyval": [],
    "graphicx": [],
    "ifpdf": [],
    "ifxetex": [],
    "environ": []
  },
  "macros": {
    "gmpoptions{}": {
      "snippet": "gmpoptions{${1:options}}"
    },
    "usempxclass{}": {
      "snippet": "usempxclass{${1:class}}"
    },
    "usempxclass[]{}": {
      "snippet": "usempxclass[${2:options}]{${1:class}}"
    },
    "usempxpackage{}": {
      "snippet": "usempxpackage{${1:package}}"
    },
    "usempxpackage[]{}": {
      "snippet": "usempxpackage[${2:options}]{${1:package}}"
    },
    "resetmpxpackages": {},
    "mpxcommands{}": {
      "snippet": "mpxcommands{${1:commands}}"
    },
    "resetmpxcommands": {},
    "mpdim{}": {
      "snippet": "mpdim{${1:dimen param}}"
    },
    "usempost{}": {
      "snippet": "usempost{${1:string}}"
    },
    "usempost[]{}": {
      "snippet": "usempost[${2:options}]{${1:string}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "btex": {},
    "verbatimtex": {}
  },
  "envs": {
    "mpost": {},
    "mpost[]": {
      "name": "mpost",
      "snippet": "[${1:options}]",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "mpost*": {},
    "mpost*[]": {
      "name": "mpost*",
      "snippet": "[${1:options}]",
      "keyvalindex": 1,
      "keyvalpos": 0
    }
  },
  "options": [
    "tex",
    "latex",
    "noshellescape",
    "shellescape",
    "nowrite",
    "write",
    "envname=${1:envname}",
    "extension=${1:extension}",
    "everymp={${1:metapost code}}",
    "clean=",
    "rmcommand=${1:cmd}",
    "postrmcommand=${1:cmd}"
  ],
  "keyvals": [
    [
      "alt={${1:alt text}}",
      "bb=${1:llx lly urx ury}",
      "bbllx=",
      "bblly=",
      "bburx=",
      "bbury=",
      "natwidth=",
      "natheight=",
      "hiresbb",
      "pagebox=",
      "viewport=${1:llx lly urx ury}",
      "trim=${1:llx lly urx ury}",
      "angle=${1:degrees}",
      "origin=",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}",
      "clip",
      "draft",
      "type=${1:file type}",
      "ext=${1:file extension}",
      "read=${1:read-file extension}",
      "command=",
      "quiet",
      "page=${1:page number}",
      "interpolate",
      "decodearray={${1:color array}}"
    ],
    [
      "name=${1:string}",
      "use",
      "mpmem=${1:metapost format}",
      "mpxprogram=",
      "mpxcommands=${1:commands}",
      "mpsettings={${1:metapost code}}",
      "runs=${1:number}"
    ]
  ]
}