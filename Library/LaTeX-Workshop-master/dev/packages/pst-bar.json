{
  "includes": {
    "pstricks": [],
    "pst-plot": [],
    "pst-xkey": []
  },
  "macros": {
    "readpsbardata{}{}": {
      "snippet": "readpsbardata{${1:macro}}{${2:filename}}"
    },
    "readpsbardata[]{}{}": {
      "snippet": "readpsbardata[${3:options}]{${1:macro}}{${2:filename}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "psbarchart{}": {
      "snippet": "psbarchart{${1:data macro}}"
    },
    "psbarchart[]{}": {
      "snippet": "psbarchart[${2:options}]{${1:data macro}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "newpsbarstyle{}{}": {
      "snippet": "newpsbarstyle{${1:name}}{${2:definition}}"
    },
    "psbarlabel{}": {
      "snippet": "psbarlabel{${1:label}}"
    },
    "psbarlabelsep": {},
    "setbarstyle{}": {
      "snippet": "setbarstyle{${1:style}}"
    },
    "trimb": {},
    "trimc": {},
    "PSTBarLoaded": {}
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "header"
    ],
    [
      "chartstyle=",
      "barstyle={${1:style list}}",
      "barcolsep=${1:factor}",
      "barsep=${1:factor}",
      "barlabelrot=${1:degrees}",
      "orientation="
    ]
  ]
}