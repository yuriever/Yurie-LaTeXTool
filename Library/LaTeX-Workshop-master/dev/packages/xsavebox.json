    },
    "xsavebox*{}[]{}": {
      "snippet": "xsavebox*{${1:name}}[${2:width}]{${3:content}}"
    },
    "xsavebox*{}[][]{}": {
      "snippet": "xsavebox*{${1:name}}[${2:width}][${3:position}]{${4:content}}"
