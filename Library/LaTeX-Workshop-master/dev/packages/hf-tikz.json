{
  "includes": {
    "tikz": [],
    "tikzlibraryshadings": [],
    "etoolbox": []
  },
  "macros": {
    "tikzmarkin{}": {
      "snippet": "tikzmarkin{${1:marker-id}}"
    },
    "tikzmarkin[]{}": {
      "snippet": "tikzmarkin[${2:options}]{${1:marker-id}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "tikzmarkend{}": {
      "snippet": "tikzmarkend{${1:marker-id}}"
    },
    "thejumping": {
      "option": "beamer"
    },
    "hfsetfillcolor{}": {
      "snippet": "hfsetfillcolor{${1:color}}",
      "option": "customcolors"
    },
    "hfsetbordercolor{}": {
      "snippet": "hfsetbordercolor{${1:color}}",
      "option": "customcolors"
    },
    "ifshowmarkers": {
      "option": "markings"
    },
    "showmarkerstrue": {
      "option": "markings"
    },
    "showmarkersfalse": {
      "option": "markings"
    },
    "fcol": {},
    "bcol": {},
    "savepointas{}{}": {
      "snippet": "savepointas{${1:name}}{${2:point}}"
    },
    "oldsavepointas{}{}": {
      "snippet": "oldsavepointas{${1:name}}{${2:point}}"
    },
    "pgfsyspdfmark{}{}{}": {
      "snippet": "pgfsyspdfmark{${1:arg1}}{${2:arg2}}{${3:arg3}}"
    },
    "oldpgfsyspdfmark{}{}{}": {
      "snippet": "oldpgfsyspdfmark{${1:arg1}}{${2:arg2}}{${3:arg3}}"
    }
  },
  "envs": {},
  "options": [
    "nofill",
    "customcolors",
    "shade",
    "beamer",
    "norndcorners",
    "markings"
  ],
  "keyvals": [
    [
      "left offset=",
      "right offset=",
      "above offset=",
      "below offset=",
      "below right offset=",
      "above left offset=",
      "top color=",
      "top color=",
      "bottom color=",
      "left color=",
      "right color=",
      "outer color=",
      "inner color=",
      "disable rounded corners",
      "set fill color=",
      "set border color=",
      "mark at=${1:position}",
      "use marker id=${1:id number}",
      "show markers",
      "marker size=",
      "marker color="
    ]
  ]
}