      "snippet": "showocgs[${3:option}]{${1:layerid1 layerid2 ...}}{${2:display}}"
    },
    "hideocgs{}{}": {
      "snippet": "hideocgs{${1:layerid1 layerid2 ...}}{${2:display}}"
