    "hideocgs[]{}{}": {
      "snippet": "hideocgs[${3:option}]{${1:layerid1 layerid2 ...}}{${2:display}}"
    },
    "setocgs{}{}{}{}": {
      "snippet": "setocgs{${1:tlayerid1 ...}}{${2:slayerid1 ...}}{${3:hlayerid1 ...}}{${4:display}}"
