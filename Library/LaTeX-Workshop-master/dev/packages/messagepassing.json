    },
    "colouredboxcolour{}": {
      "snippet": "colouredboxcolour{${1:color}}"
    },
    "oobcolour{}": {
      "snippet": "oobcolour{${1:color}}"
    },
    "themaxtime": {},
    "theprocessnb": {},
    "iftimeline": {},
    "timelinetrue": {},
    "timelinefalse": {}
