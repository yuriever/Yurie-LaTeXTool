{
  "includes": {
    "tikz": [],
    "tikzlibraryquotes": [],
    "tikzlibrarycalc": [],
    "float": []
  },
  "macros": {
    "newprocess{}": {
      "snippet": "newprocess{${1:name}}"
    },
    "newprocess{}[]": {
      "snippet": "newprocess{${1:name}}[${2:display name}]"
    },
    "newprocesswithlength{}{}": {
      "snippet": "newprocesswithlength{${1:name}}{${2:length}}"
    },
    "newprocesswithlength{}[]{}": {
      "snippet": "newprocesswithlength{${1:name}}[${2:display name}]{${3:length}}"
    },
    "processlength{}{}": {
      "snippet": "processlength{${1:name}}{${2:length}}"
    },
    "newprocesswithstateinterval{}{}": {
      "snippet": "newprocesswithstateinterval{${1:name}}{${2:state name}}"
    },
    "newprocesswithstateinterval{}[]{}": {
      "snippet": "newprocesswithstateinterval{${1:name}}[${2:display name}]{${3:state name}}"
    },
    "newprocesswithcrash{}{}{}": {
      "snippet": "newprocesswithcrash{${1:name}}{${2:length}}{${3:crash coord name}}"
    },
    "newprocesswithcrash{}[]{}{}": {
      "snippet": "newprocesswithcrash{${1:name}}[${2:display name}]{${3:length}}{${4:crash coord name}}"
    },
    "drawtimeline{}": {
      "snippet": "drawtimeline{${1:length}}"
