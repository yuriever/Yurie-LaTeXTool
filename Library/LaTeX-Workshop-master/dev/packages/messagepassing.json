  "envs": {
    "messagepassing": {},
    "messagepassing[]": {
      "name": "messagepassing",
      "snippet": "[${1:TikZ options}]"
    },
    "messagepassing[][]": {
      "name": "messagepassing",
      "snippet": "[${1:TikZ options}][${2:caption}]"
    },
    "messagepassing[][][]": {
