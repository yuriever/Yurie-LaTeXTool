      "snippet": "crash{${1:process}}{${2:time}}{${3:name}}"
    },
    "restart{}{}{}": {
      "snippet": "restart{${1:name}}{${2:date}}{${3:duration}}"
    },
    "checkpoint{}{}{}": {
      "snippet": "checkpoint{${1:process}}{${2:time}}{${3:name}}"
    },
    "checkpointspecial{}{}{}{}": {
      "snippet": "checkpointspecial{${1:process}}{${2:time}}{${3:name}}{${4:label}}"
    },
    "stateinterval{}{}{}": {
      "snippet": "stateinterval{${1:process}}{${2:time}}{${3:name}}"
