{
  "includes": {
    "keyval": [],
    "kvsetkeys": [],
    "thm-patch": []
  },
  "macros": {
    "declaretheoremstyle{}": {
      "snippet": "declaretheoremstyle{${1:style name}}"
    },
    "declaretheoremstyle[]{}": {
      "snippet": "declaretheoremstyle[${2:options}]{${1:style name}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "declaretheorem{}": {
      "snippet": "declaretheorem{${1:envname}}"
    },
    "declaretheorem[]{}": {
      "snippet": "declaretheorem[${2:options}]{${1:envname}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "declaretheorem[]{}[]": {
      "snippet": "declaretheorem[${2:options}]{${1:envname}}[${3:options}]",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "declaretheorem{}[]": {
      "snippet": "declaretheorem{${1:envname}}[${2:options}]",
      "keyvalindex": 1,
      "keyvalpos": 1
    },
    "thmcontinues{}": {
      "snippet": "thmcontinues{${1:key}}"
    }
  },
  "envs": {},
  "options": [
    "lowercase",
    "uppercase",
    "anycase"
  ],
  "keyvals": [
    [
      "spaceabove=",
      "spacebelow=",
      "headfont=${1:font commands}",
      "notefont=${1:font commands}",
      "bodyfont=${1:font commands}",
      "headpunct=${1:symbol}",
      "notebraces=${1:two chars}",
      "break",
      "postheadspace=",
      "headformat=${1:code}",
      "headstyle=${1:code}",
      "headindent=",
      "parent=${1:counter}",
      "numberwithin=${1:counter}",
      "within=${1:counter}",
      "sibling=${1:counter}",
      "numberlike=${1:counter}",
      "sharenumber=${1:counter}",
      "title=${1:title}",
      "name=${1:name}",
      "heading=${1:heading}",
      "numbered=",
      "style=${1:style name}",
      "preheadhook=${1:code}",
      "postheadhook=${1:code}",
      "prefoothook=${1:code}",
      "postfoothook=${1:code}",
      "refname=${1:name}",
      "refname={${1:singular-name,plural-name}}",
      "Refname=${1:name}",
      "Refname={${1:singular-name,plural-name}}",
      "shaded={${1:shade options}}",
      "thmbox=",
      "mdframed={${1:mdframed options}}",
      "qed=${1:symbol}"
    ],
    [
      "parent=${1:counter}",
      "numberwithin=${1:counter}",
      "within=${1:counter}",
      "sibling=${1:counter}",
      "numberlike=${1:counter}",
      "sharenumber=${1:counter}",
      "title=${1:title}",
      "name=${1:name}",
      "heading=${1:heading}",
      "numbered=",
      "style=${1:style name}",
      "preheadhook=${1:code}",
      "postheadhook=${1:code}",
      "prefoothook=${1:code}",
      "postfoothook=${1:code}",
      "refname=${1:name}",
      "refname={${1:singular-name,plural-name}}",
      "Refname=${1:name}",
      "Refname={${1:singular-name,plural-name}}",
      "shaded={${1:shade options}}",
      "thmbox=",
      "mdframed={${1:mdframed options}}",
      "qed=${1:symbol}"
    ]
  ]
}