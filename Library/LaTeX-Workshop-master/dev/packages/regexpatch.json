{
  "includes": {
    "patch-common": []
  },
  "macros": {
    "xpatchcmd*{}{}{}{}{}": {
      "snippet": "xpatchcmd*{${1:command}}{${2:search}}{${3:replace all}}{${4:success}}{${5:failure}}"
    },
    "xpatchbibmacro*{}{}{}{}{}": {
      "snippet": "xpatchbibmacro*{${1:name}}{${2:search}}{${3:replace all}}{${4:success}}{${5:failure}}"
    },
    "xpatchbibdriver*{}{}{}{}{}": {
      "snippet": "xpatchbibdriver*{${1:name}}{${2:search}}{${3:replace all}}{${4:success}}{${5:failure}}"
    },
    "xpatchfieldformat*[]{}{}{}{}{}": {
      "snippet": "xpatchfieldformat*[${6:entry type}]{${1:name}}{${2:search}}{${3:replace all}}{${4:success}}{${5:failure}}"
    },
    "xpatchnameformat*[]{}{}{}{}{}": {
      "snippet": "xpatchnameformat*[${6:entry type}]{${1:name}}{${2:search}}{${3:replace all}}{${4:success}}{${5:failure}}"
    },
    "xpatchlistformat*[]{}{}{}{}{}": {
      "snippet": "xpatchlistformat*[${6:entry type}]{${1:name}}{${2:search}}{${3:replace all}}{${4:success}}{${5:failure}}"
    },
    "xpatchindexfieldformat*[]{}{}{}{}{}": {
      "snippet": "xpatchindexfieldformat*[${6:entry type}]{${1:name}}{${2:search}}{${3:replace all}}{${4:success}}{${5:failure}}"
    },
    "xpatchindexnameformat*[]{}{}{}{}{}": {
      "snippet": "xpatchindexnameformat*[${6:entry type}]{${1:name}}{${2:search}}{${3:replace all}}{${4:success}}{${5:failure}}"
    },
    "xpatchindexlistformat*[]{}{}{}{}{}": {
      "snippet": "xpatchindexlistformat*[${6:entry type}]{${1:name}}{${2:search}}{${3:replace all}}{${4:success}}{${5:failure}}"
    },
    "regexpatchcmd{}{}{}{}{}": {
      "snippet": "regexpatchcmd{${1:command}}{${2:search}}{${3:replace once}}{${4:success}}{${5:failure}}"
    },
    "regexpatchcmd*{}{}{}{}{}": {
      "snippet": "regexpatchcmd*{${1:command}}{${2:search}}{${3:replace all}}{${4:success}}{${5:failure}}"
    },
    "regexpatchbibmacro{}{}{}{}{}": {
      "snippet": "regexpatchbibmacro{${1:name}}{${2:search}}{${3:replace once}}{${4:success}}{${5:failure}}"
    },
    "regexpatchbibmacro*{}{}{}{}{}": {
      "snippet": "regexpatchbibmacro*{${1:name}}{${2:search}}{${3:replace all}}{${4:success}}{${5:failure}}"
    },
    "xshowcmd*{}": {
      "snippet": "xshowcmd*{${1:command}}"
    },
    "xpatchoptarg{}{}": {
      "snippet": "xpatchoptarg{${1:name}}{${2:replace}}"
    },
    "xpatchparametertext{}{}{}{}{}": {
      "snippet": "xpatchparametertext{${1:name}}{${2:search}}{${3:replace}}{${4:success}}{${5:failure}}"
    },
    "checkpatchable{}": {
      "snippet": "checkpatchable{${1:name}}"
    },
    "tracingxpatches": {},
    "tracingxpatches[]": {
      "snippet": "tracingxpatches[${1:0 or 1}]"
    }
  },
  "envs": {},
  "options": [],
  "keyvals": []
}