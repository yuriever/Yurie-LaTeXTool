{
  "includes": {
    "graphicx": [],
    "hyperref": [],
    "amsmath": [],
    "subcaption": [],
    "enumitem": [],
    "verbatim": [],
    "ulem": [],
    "mdframed": [],
    "tikz": []
  },
  "macros": {
    "set{}{}": {
      "snippet": "set{${1:attribute}}{${2:value}}"
    },
    "get{}": {
      "snippet": "get{${1:attribute}}"
    },
    "mn{}": {
      "snippet": "mn{${1:keyvals}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "alt{}": {
      "snippet": "alt{${1:term}}"
    },
    "deprecated{}": {
      "snippet": "deprecated{${1:term}}"
    },
    "domain{}": {
      "snippet": "domain{${1:domain}}"
    },
    "mncite{}": {
      "snippet": "mncite{${1:key}}"
    },
    "mncite[]{}": {
      "snippet": "mncite[${2:add. text}]{${1:key}}"
    },
    "mncite{}[]": {
      "snippet": "mncite{${1:key}}[${2:localities}]"
    },
    "mncite[]{}[]": {
      "snippet": "mncite[${2:add. text}]{${1:key}}[${3:localities}]"
    },
    "att{}": {
      "snippet": "att{${1:arg}}"
    },
    "att[]{}": {
      "snippet": "att[${2:opt}]{${1:arg}}"
    },
    "lxRDFa{}": {
      "snippet": "lxRDFa{${1:arg}}"
    },
    "lxRDFa[]{}": {
      "snippet": "lxRDFa[${2:opt}]{${1:arg}}"
    },
    "lxRDF{}": {
      "snippet": "lxRDF{${1:arg}}"
    },
    "lxRDF[]{}": {
      "snippet": "lxRDF[${2:opt}]{${1:arg}}"
    }
  },
  "envs": {
    "example": {},
    "note": {},
    "source": {},
    "key": {},
    "requirement": {},
    "recommendation": {},
    "permission": {},
    "specification": {},
    "verification": {},
    "import": {},
    "tip": {},
    "important": {},
    "caution": {},
    "warning": {}
  },
  "options": [],
  "keyvals": [
    [
      "heading=${1:heading}",
      "level=${1:level}",
      "obligation=",
      "language=${1:lang code}",
      "script=${1:script name}",
      "reviewer=\"${1:name}\"",
      "from=${1:anchor}",
      "to=${1:anchor}",
      "date=${1:date}"
    ]
  ]
}