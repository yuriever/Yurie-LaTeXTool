{
  "includes": {},
  "macros": {
    "pangram": {},
    "pangram[]": {
      "snippet": "pangram[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "PangramSetup{}": {
      "snippet": "PangramSetup{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "NewPangramClass{}{}": {
      "snippet": "NewPangramClass{${1:text class}}{${2:text}}"
    }
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "textclass=${1:text-class}",
      "sizes={${1:size1,size2,...}}",
      "linegap=",
      "tagskip=",
      "tagfont=${1:font commands}",
      "font=${1:font commands}",
      "before=${1:code}",
      "after=${1:code}"
    ]
  ]
}